	<xs:simpleType name="data-format-type">
		<xs:annotation>
			<xs:documentation>"docdb": standardised following the DOCDB primary standardisation rules;</xs:documentation>
			<xs:documentation>"docda": standardised following the DOCDB secondary standardisation rules (translation upper-case);</xs:documentation>
			<xs:documentation>"epodoc": format as represented in espacenet;</xs:documentation>
			<xs:documentation>"original": as supplied by the provider (converted to UTF-8);</xs:documentation>
			<xs:documentation>"lni": as supplied by LexisNexis;</xs:documentation>
			<xs:documentation>"lookup": looked up following LexisNexis rules;</xs:documentation>
			<xs:documentation>"translated": translated following LexisNexis rules</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:pattern value="docdb"/>
			<xs:pattern value="docdba"/>
			<xs:pattern value="epodoc"/>
			<xs:pattern value="original"/>
			<xs:pattern value="lni"/>
			<xs:pattern value="lookup"/>
			<xs:pattern value="translated"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="date-type">
		<xs:annotation>
			<xs:documentation>Components of a date.</xs:documentation>
			<xs:documentation>(Format: YYYYMMDD/YYYYMM00/YYYY0000/00000000)</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:pattern value="([1][0-9]|[2][0])[0-9]{2}([0][1-9]|[1][0-2])[0-9]{2}"/>
			<xs:pattern value="([1][0-9]|[2][0])[0-9]{2}([0][1-9]|[1][0-2])[0]{2}"/>
			<xs:pattern value="([1][0-9]|[2][0])[0-9]{2}[0]{2}[0]{2}"/>
			<xs:pattern value="[0]{4}[0]{2}[0]{2}"/>
		</xs:restriction>
