		<xs:simpleType>
			<xs:restriction base="text-type"/>
		</xs:simpleType>
	</xs:element>
	<xs:element name="tgroup">
		<xs:annotation>
			<xs:documentation>Table group.</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:choice minOccurs="0" maxOccurs="unbounded">
				<xs:element ref="colspec" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="thead" minOccurs="0"/>
				<xs:element ref="tbody"/>
			</xs:choice>
			<xs:attribute name="cols" type="xs:NMTOKEN" use="required"/>
			<xs:attribute name="colsep" type="xs:NMTOKEN"/>
			<xs:attribute name="rowsep" type="xs:NMTOKEN"/>
			<xs:attribute name="align" type="align-type"/>
			<xs:attribute name="id" type="xs:ID"/>
		</xs:complexType>
	</xs:element>
	<xs:element name="thead">
		<xs:annotation>
			<xs:documentation>Table header.</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="row" maxOccurs="unbounded"/>
			</xs:sequence>
			<xs:attribute name="valign" type="valign-type"/>
			<xs:attribute name="id" type="xs:ID"/>
		</xs:complexType>
	</xs:element>
	<xs:element name="therapeutic-equivalence">
		<xs:annotation>
			<xs:documentation>OrangeBook Therapeutic Equivalence.</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:simpleContent>
				<xs:extension base="text-type">
					<xs:attribute name="code" type="orangebook-therapeutic-equivalence-code" use="required"/>
				</xs:extension>
			</xs:simpleContent>
		</xs:complexType>
	</xs:element>
	<xs:element name="time">
		<xs:annotation>
			<xs:documentation>Time.</xs:documentation>
			<xs:documentation>(Format: HHMM)</xs:documentation>
		</xs:annotation>
		<xs:simpleType>
			<xs:restriction base="text-type">
				<xs:pattern value="[0-9]{4}"/>
			</xs:restriction>
		</xs:simpleType>
	</xs:element>
	<xs:element name="title">
		<xs:annotation>
			<xs:documentation>Create content for title element in table.</xs:documentation>
		</xs:annotation>
		<xs:complexType mixed="true">
			<xs:choice minOccurs="0" maxOccurs="unbounded">
				<xs:element ref="b"/>
				<xs:element ref="i"/>
				<xs:element ref="u"/>
				<xs:element ref="o"/>
				<xs:element ref="sup"/>
				<xs:element ref="sub"/>
				<xs:element ref="smallcaps"/>
				<xs:element ref="br"/>
			</xs:choice>
			<xs:attribute name="id" type="xs:ID"/>
			<xs:attribute name="align" type="halign-type"/>
		</xs:complexType>
	</xs:element>
	<xs:element name="title-of-invention">
		<xs:annotation>
			<xs:documentation>(US) Pair title of invention.</xs:documentation>
		</xs:annotation>
		<xs:simpleType>
			<xs:restriction base="text-type"/>
		</xs:simpleType>
	</xs:element>
	<xs:element name="total-pta-adjustment">
		<xs:annotation>
			<xs:documentation>(US) Pair total pta adjustment.</xs:documentation>
		</xs:annotation>
		<xs:simpleType>
			<xs:restriction base="text-type"/>
		</xs:simpleType>
	</xs:element>
	<xs:element name="total-pte-extension">
		<xs:annotation>
			<xs:documentation>(US) Pair total pte extensions.</xs:documentation>
		</xs:annotation>
		<xs:simpleType>
			<xs:restriction base="text-type"/>
		</xs:simpleType>
	</xs:element>
	<xs:element name="transaction-date">
		<xs:annotation>
			<xs:documentation>Post-issuance daily assignment transaction date.</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="date"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="transaction-history">
		<xs:annotation>
			<xs:documentation>(US) Pair transaction history.</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence maxOccurs="unbounded">
				<xs:element name="entry">
					<xs:complexType>
						<xs:simpleContent>
							<xs:extension base="text-type">
								<xs:attribute name="sequence" type="sequence-type" use="required"/>
								<xs:attribute name="date" type="date-type"/>
							</xs:extension>
						</xs:simpleContent>
					</xs:complexType>
				</xs:element>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="translation-submission-date">
		<xs:annotation>
			<xs:documentation>Translation submission date.</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="date"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="type-of-correction">
		<xs:annotation>
			<xs:documentation>Type of correction.</xs:documentation>
			<xs:documentation>(ST.32 equivalency = B667)</xs:documentation>
		</xs:annotation>
		<xs:simpleType>
			<xs:restriction base="text-type"/>
		</xs:simpleType>
	</xs:element>
	<xs:element name="u">
		<xs:annotation>
			<xs:documentation>Underscore: style - single is default.</xs:documentation>
		</xs:annotation>
		<xs:complexType mixed="true">
			<xs:choice minOccurs="0" maxOccurs="unbounded">
				<xs:element ref="b"/>
				<xs:element ref="i"/>
				<xs:element ref="o"/>
				<xs:element ref="sub"/>
				<xs:element ref="sup"/>
				<xs:element ref="smallcaps"/>
			</xs:choice>
			<xs:attribute name="style" type="underscore-type" default="single"/>
		</xs:complexType>
	</xs:element>
	<xs:element name="ul">
		<xs:annotation>
			<xs:documentation>Unordered Lists: list-style may be empty for lists with no bullet or other symbol default list-style is 'bullets'.</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="li" maxOccurs="unbounded"/>
			</xs:sequence>
			<xs:attribute name="id" type="xs:ID"/>
			<xs:attribute name="list-style" type="list-style-type" default="bullet"/>
			<xs:attribute name="compact">
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:enumeration value="compact"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
		</xs:complexType>
	</xs:element>
	<xs:element name="unexamined-not-printed-without-grant">
		<xs:annotation>
			<xs:documentation>Unexamined, not printed document without grant.</xs:documentation>
			<xs:documentation>(ST.32 equivalency = B410)</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="date"/>
				<xs:element ref="text" minOccurs="0"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="unexamined-printed-without-grant">
		<xs:annotation>
			<xs:documentation>Unexamined printed document without grant.</xs:documentation>
			<xs:documentation>(ST.32 equivalency = B430)</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="date"/>
				<xs:element ref="text" minOccurs="0"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="unlinked-indexing-code">
		<xs:annotation>
			<xs:documentation>Unlinked indexing code.</xs:documentation>
			<xs:documentation>(ST.32 equivalency = B515/B525)</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:group ref="ipc-classification-group"/>
			</xs:sequence>
			<xs:attribute name="sequence" type="sequence-type"/>
		</xs:complexType>
	</xs:element>
	<xs:element name="url">
		<xs:annotation>
			<xs:documentation>Universal Resource Locator; e.g. www.wipo.int.</xs:documentation>
		</xs:annotation>
		<xs:simpleType>
			<xs:restriction base="text-type"/>
		</xs:simpleType>
	</xs:element>
	<xs:element name="us-371c124-date">
		<xs:annotation>
			<xs:documentation>PCT section 371 date.</xs:documentation>
			<xs:documentation>(ST.32 equivalency = B863)</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="date"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="us-claim-statement">
		<xs:annotation>
			<xs:documentation>Claim statement, ie. "What I claim is:".</xs:documentation>
		</xs:annotation>
		<xs:complexType mixed="true">
			<xs:choice minOccurs="0" maxOccurs="unbounded">
				<xs:element ref="b"/>
				<xs:element ref="i"/>
				<xs:element ref="o"/>
				<xs:element ref="u"/>
				<xs:element ref="sup"/>
				<xs:element ref="sub"/>
				<xs:element ref="smallcaps"/>
				<xs:element ref="br"/>
			</xs:choice>
		</xs:complexType>
	</xs:element>
	<xs:element name="us-relation">
		<xs:annotation>
			<xs:documentation>If there is more than one divisional reissue, siblings are indicated by multiple occurences of child.</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="parent-doc"/>
				<xs:element ref="child-doc" maxOccurs="unbounded"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="us-rights">
		<xs:annotation>
			<xs:documentation>This person is successor to the rights of an inventor who is deceased, insane, or legally incapacitated, for the purposes of the designation of the United States of America.</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:simpleContent>
				<xs:extension base="xs:string">
					<xs:attribute name="to-dead-inventor" type="xs:string" use="required"/>
					<xs:attribute name="kind" type="rights-kind-type" use="required"/>
				</xs:extension>
			</xs:simpleContent>
		</xs:complexType>
	</xs:element>
	<xs:element name="us-term-extension">
		<xs:annotation>
			<xs:documentation>Indicates that the term of the patent has been extended under 35 USC 154(b) by showing the length of the extension. The content of this tag must be literally either "5 years", or the number of days (as an integer) if the extension is less than five years.</xs:documentation>
			<xs:documentation>(ST.32 equivalency = B474US)</xs:documentation>
		</xs:annotation>
		<xs:simpleType>
			<xs:restriction base="text-type"/>
		</xs:simpleType>
	</xs:element>
	<xs:element name="uspto-adjustment">
		<xs:annotation>
			<xs:documentation>(US) Pair USPTO adjustments.</xs:documentation>
		</xs:annotation>
		<xs:simpleType>
			<xs:restriction base="text-type"/>
		</xs:simpleType>
	</xs:element>
	<xs:element name="uspto-delay-pto">
		<xs:annotation>
			<xs:documentation>(US) Pair USPTO delay.</xs:documentation>
		</xs:annotation>
		<xs:simpleType>
			<xs:restriction base="text-type"/>
		</xs:simpleType>
	</xs:element>
	<xs:element name="utility-model-basis">
		<xs:annotation>
			<xs:documentation>Document on which utility model is based.</xs:documentation>
			<xs:documentation>(ST.32 equivalency = B670)</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="relation"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="variety">
		<xs:annotation>
			<xs:documentation>The plant variety name.</xs:documentation>
		</xs:annotation>
		<xs:simpleType>
			<xs:restriction base="xs:string">
				<xs:minLength value="1"/>
			</xs:restriction>
		</xs:simpleType>
	</xs:element>
	<xs:element name="vid">
		<xs:annotation>
			<xs:documentation>Volume id/number.</xs:documentation>
		</xs:annotation>
		<xs:simpleType>
			<xs:restriction base="text-type"/>
		</xs:simpleType>
	</xs:element>
	<xs:element name="withdrawn">
		<xs:annotation>
			<xs:documentation>Demand withdrawn by a notice from applicant.</xs:documentation>
			<xs:documentation>(PCT Article 37; PCT Rule 90bis.4)</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="date"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="withdrawn-date">
		<xs:annotation>
			<xs:documentation>Legal withdrawn date.</xs:documentation>
			<xs:documentation>(ST.32 equivalency = L526EP)</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="date"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<!--  ********** Group Elements ********** 
  -->
	<xs:group name="classification-additional-group">
		<xs:choice>
			<xs:element ref="additional-info" minOccurs="0" maxOccurs="unbounded"/>
			<xs:element ref="linked-indexing-code-group" minOccurs="0" maxOccurs="unbounded"/>
			<xs:element ref="unlinked-indexing-code" minOccurs="0" maxOccurs="unbounded"/>
		</xs:choice>
	</xs:group>
	<xs:group name="classification-group">
		<xs:annotation>
			<xs:documentation>Classification group.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element ref="section"/>
			<xs:element ref="class"/>
			<xs:element ref="subclass"/>
			<xs:element ref="main-group" minOccurs="0"/>
			<xs:element ref="subgroup" minOccurs="0"/>
		</xs:sequence>
	</xs:group>
	<xs:group name="cpc-classification-group">
		<xs:annotation>
			<xs:documentation>CPC Classification group.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="section" type="cpc-classification-type"/>
			<xs:element name="class" type="cpc-classification-type"/>
			<xs:element name="subclass" type="cpc-classification-type"/>
			<xs:element name="main-group" type="cpc-classification-type" minOccurs="0"/>
			<xs:element name="subgroup" type="cpc-classification-type" minOccurs="0"/>
		</xs:sequence>
	</xs:group>
	<xs:group name="ipc-classification-group">
		<xs:annotation>
			<xs:documentation>IPC classification group.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element ref="text"/>
			<xs:sequence minOccurs="0">
				<xs:element ref="edition" minOccurs="0"/>
				<xs:group ref="classification-group"/>
				<xs:element ref="qualifying-character" minOccurs="0"/>
			</xs:sequence>
		</xs:sequence>
	</xs:group>
	<xs:group name="name-group">
		<xs:sequence>
			<xs:choice>
				<xs:sequence>
					<xs:element ref="name"/>
					<xs:element ref="name-standardized" minOccurs="0"/>
					<xs:element ref="name-normalized" minOccurs="0"/>
				</xs:sequence>
				<xs:sequence>
					<xs:element ref="prefix" minOccurs="0"/>
					<xs:choice>
						<xs:element ref="last-name"/>
						<xs:sequence>
							<xs:element ref="orgname"/>
							<xs:element ref="orgname-standardized" minOccurs="0"/>
							<xs:element ref="orgname-normalized" minOccurs="0"/>
						</xs:sequence>
					</xs:choice>
					<xs:element ref="first-name" minOccurs="0"/>
					<xs:element ref="middle-name" minOccurs="0"/>
					<xs:element ref="suffix" minOccurs="0"/>
					<xs:element ref="iid" minOccurs="0"/>
					<xs:element ref="role" minOccurs="0"/>
					<xs:element ref="orgname" minOccurs="0"/>
					<xs:element ref="department" minOccurs="0"/>
					<xs:element ref="synonym" minOccurs="0" maxOccurs="unbounded"/>
				</xs:sequence>
			</xs:choice>
			<xs:element ref="registered-number" minOccurs="0"/>
			<xs:element ref="issuing-office" minOccurs="0"/>
		</xs:sequence>
	</xs:group>
	<xs:group name="national-classification-group">
		<xs:annotation>
			<xs:documentation>National classification group.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element ref="text"/>
			<xs:sequence minOccurs="0">
				<xs:element ref="edition" minOccurs="0"/>
				<xs:element ref="section" minOccurs="0"/>
				<xs:element ref="class"/>
				<xs:element ref="subclass" minOccurs="0"/>
				<xs:element ref="main-group" minOccurs="0"/>
				<xs:element ref="subgroup" minOccurs="0"/>
				<xs:element ref="code" minOccurs="0"/>
			</xs:sequence>
		</xs:sequence>
	</xs:group>
	<!--  ********** Complex Types ********** 
  -->
	<xs:complexType name="cpc-classification-type">
		<xs:annotation>
			<xs:documentation>CPC classification element with option description.</xs:documentation>
		</xs:annotation>
		<xs:simpleContent>
			<xs:extension base="xs:string">
				<xs:attribute name="descr" type="xs:string"/>
			</xs:extension>
		</xs:simpleContent>
	</xs:complexType>
	<xs:complexType name="ipc-further-classification-type">
		<xs:annotation>
			<xs:documentation>IPC further classification type.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:group ref="ipc-classification-group"/>
		</xs:sequence>
		<xs:attribute name="sequence" type="sequence-type"/>
	</xs:complexType>
	<xs:complexType name="ipc-main-classification-type">
		<xs:annotation>
			<xs:documentation>IPC main classification type.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:group ref="ipc-classification-group"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="locarno-classification-type">
		<xs:annotation>
			<xs:documentation>Locarno classification.</xs:documentation>
			<xs:documentation>(ST.32 equivalency = B512/B522)</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element ref="text"/>
			<xs:sequence minOccurs="0">
				<xs:element ref="class"/>
				<xs:element ref="subclass" minOccurs="0"/>
			</xs:sequence>
		</xs:sequence>
		<xs:attribute name="sequence" type="sequence-type"/>
	</xs:complexType>
	<xs:complexType name="national-further-classification-type">
		<xs:annotation>
			<xs:documentation>Further/secondary classification.</xs:documentation>
			<xs:documentation>(ST.32 equivalency = B512/B522)</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:group ref="national-classification-group"/>
			<xs:element ref="facet" minOccurs="0"/>
		</xs:sequence>
		<xs:attribute name="sequence" type="sequence-type"/>
	</xs:complexType>
	<xs:complexType name="national-main-classification-type">
		<xs:annotation>
			<xs:documentation>Main classification.</xs:documentation>
			<xs:documentation>(ST.32 equivalency = B511/B521)</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:group ref="national-classification-group"/>
			<xs:element ref="facet" minOccurs="0"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="post-issuance-element-type">
		<xs:annotation>
			<xs:documentation>US Post-issuance data element.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element ref="date"/>
			<xs:element ref="comment"/>
			<xs:element ref="official-gazette" minOccurs="0"/>
		</xs:sequence>
		<xs:attribute name="id" type="xs:ID"/>
		<xs:attribute name="sequence" type="sequence-type"/>
	</xs:complexType>
	<!--  Attribute Groups 
  -->
	<xs:attributeGroup name="equivalent-source">
		<xs:annotation>
			<xs:documentation>Equivalent document identifier for title, abstract, description and claims.</xs:documentation>
		</xs:annotation>
		<xs:attribute name="generated" type="xs:boolean"/>
		<xs:attribute name="country" type="xs:string"/>
		<xs:attribute name="doc-number" type="xs:string"/>
		<xs:attribute name="kind" type="xs:string"/>
	</xs:attributeGroup>
	<!--  ********** Simple Types ********** 
  -->
	<xs:simpleType name="align-type">
		<xs:annotation>
			<xs:documentation>Alignment enumeration type.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:enumeration value="left"/>
			<xs:enumeration value="right"/>
			<xs:enumeration value="center"/>
			<xs:enumeration value="justify"/>
			<xs:enumeration value="char"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="amendment-type">
		<xs:annotation>
			<xs:documentation>Amendment enumeration type.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:enumeration value="u7"/>
			<xs:enumeration value="normal"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="applicant-role-type">
		<xs:annotation>
			<xs:documentation>Applicant role enumeration type.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:enumeration value="corporate"/>
			<xs:enumeration value="government"/>
			<xs:enumeration value="hospital"/>
			<xs:enumeration value="nonprofit"/>
			<xs:enumeration value="university"/>
			<xs:enumeration value="individual"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="applicant-type">
		<xs:annotation>
			<xs:documentation>Applicant enumeration type.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:enumeration value="applicant"/>
			<xs:enumeration value="applicant-inventor"/>
			<xs:enumeration value="assignee"/>
			<xs:enumeration value="inventor"/>
			<xs:enumeration value="legal-representative"/>
			<xs:enumeration value="obligated-assignee"/>
			<xs:enumeration value="party-of-interest"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="boolean-type">
		<xs:annotation>
			<xs:documentation>Boolean enumeration type.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:enumeration value="yes"/>
			<xs:enumeration value="no"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="carriers-type">
		<xs:annotation>
			<xs:documentation>Carriers enumeation type.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:enumeration value="internal-electronic"/>
			<xs:enumeration value="external-electronic"/>
			<xs:enumeration value="paper"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="citation-type">
		<xs:annotation>
			<xs:documentation>The TYPE of citation (office specific).</xs:documentation>
			<xs:documentation>(recommended:)</xs:documentation>
			<xs:documentation>a - Abstract citation of no specific kind</xs:documentation>
			<xs:documentation>b - Book citation</xs:documentation>
			<xs:documentation>i - Bilogical abstract citation</xs:documentation>
			<xs:documentation>c - Chemical abstracts citation</xs:documentation>
			<xs:documentation>e - Database citation</xs:documentation>
			<xs:documentation>d - Derwent citation</xs:documentation>
			<xs:documentation>p - Patent cited within NPL group</xs:documentation>
