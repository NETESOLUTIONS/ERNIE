		<xs:annotation>
			<xs:documentation>Chemical formulae, ring structures, Markush structures, etc.</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:attribute name="id" type="xs:ID"/>
			<xs:attribute name="file" type="xs:string" use="required"/>
			<xs:attribute name="chem-type" type="xs:string"/>
		</xs:complexType>
	</xs:element>
	<xs:element name="chemistry">
		<xs:annotation>
			<xs:documentation>Chemical formulae, ring structures, Markush structures, etc.</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:choice>
				<xs:element ref="img"/>
				<xs:sequence>
					<xs:element ref="chem"/>
					<xs:element ref="img" minOccurs="0"/>
				</xs:sequence>
			</xs:choice>
			<xs:attribute name="id" type="xs:ID"/>
			<xs:attribute name="num" type="xs:string"/>
		</xs:complexType>
	</xs:element>
	<xs:element name="child">
		<xs:annotation>
			<xs:documentation>(US) Pair continuity child.</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="document-id"/>
				<xs:element ref="application" minOccurs="0"/>
				<xs:element ref="status" minOccurs="0"/>
				<xs:element ref="text" minOccurs="0"/>
			</xs:sequence>
			<xs:attribute name="sequence" type="sequence-type" use="required"/>
		</xs:complexType>
	</xs:element>
	<xs:element name="child-continuity-data">
		<xs:annotation>
			<xs:documentation>(US) Pair child continuity data.</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="child" maxOccurs="unbounded"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="child-doc">
		<xs:annotation>
			<xs:documentation>Child Document.</xs:documentation>
			<xs:documentation>(ST.32 equivalency = CDOC)</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="document-id" maxOccurs="unbounded"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="citation">
		<xs:annotation>
			<xs:documentation>Grouping tag for patent literature (patcit) or non-patent literature (nplcit) plus data related to those citations.</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:choice>
					<xs:element ref="patcit"/>
					<xs:element ref="nplcit"/>
					<xs:element ref="fwdcit"/>
				</xs:choice>
				<xs:element ref="corresponding-docs" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="category" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="rel-claims" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="classification-ipc" minOccurs="0"/>
				<xs:element ref="classifications-ipcr" minOccurs="0"/>
				<xs:element ref="classification-national" minOccurs="0"/>
				<xs:element ref="classifications-cpc" minOccurs="0"/>
			</xs:sequence>
			<xs:attribute name="srep-phase" type="text-type"/>
		</xs:complexType>
	</xs:element>
	<xs:element name="citation-list">
		<xs:annotation>
			<xs:documentation>List of citations.</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:choice maxOccurs="unbounded">
				<xs:element ref="patent-literature"/>
				<xs:element ref="non-patent-literature"/>
				<xs:sequence maxOccurs="unbounded">
					<xs:element ref="heading" minOccurs="0" maxOccurs="unbounded"/>
					<xs:element ref="p" maxOccurs="unbounded"/>
				</xs:sequence>
			</xs:choice>
			<xs:attribute name="id" type="xs:ID"/>
		</xs:complexType>
	</xs:element>
	<xs:element name="city">
		<xs:annotation>
			<xs:documentation>City or town name.</xs:documentation>
		</xs:annotation>
		<xs:simpleType>
			<xs:restriction base="text-type"/>
		</xs:simpleType>
	</xs:element>
	<xs:element name="claim">
		<xs:annotation>
			<xs:documentation>Defines the matter for which protection is sought.</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="claim-text" maxOccurs="unbounded"/>
			</xs:sequence>
			<xs:attribute name="id" type="xs:ID"/>
			<xs:attribute name="num" type="xs:string"/>
			<xs:attribute name="claim-type" type="xs:string"/>
			<xs:attribute name="independent" type="xs:boolean"/>
		</xs:complexType>
	</xs:element>
	<xs:element name="claim-ref">
		<xs:annotation>
			<xs:documentation>Reference to a claim.</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:simpleContent>
				<xs:extension base="text-type">
					<xs:attribute name="idref" type="xs:IDREFS" use="required"/>
					<xs:attribute name="generated" type="xs:boolean" default="false"/>
				</xs:extension>
			</xs:simpleContent>
		</xs:complexType>
	</xs:element>
	<xs:element name="claim-statement">
		<xs:annotation>
			<xs:documentation>Defines the claim statement, i.e. "What we claim is:".</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="claim-text" maxOccurs="unbounded"/>
			</xs:sequence>
			<xs:attribute name="id" type="xs:ID"/>
			<xs:attribute name="num" type="xs:string"/>
		</xs:complexType>
	</xs:element>
	<xs:element name="claim-text">
		<xs:annotation>
			<xs:documentation>The text of the claim; also used for for sub-para indentation.</xs:documentation>
		</xs:annotation>
		<xs:complexType mixed="true">
			<xs:choice minOccurs="0" maxOccurs="unbounded">
				<xs:element ref="claim-text"/>
				<xs:element ref="claim-ref"/>
				<xs:element ref="b"/>
				<xs:element ref="i"/>
				<xs:element ref="u"/>
				<xs:element ref="o"/>
				<xs:element ref="sup"/>
				<xs:element ref="sub"/>
				<xs:element ref="smallcaps"/>
				<xs:element ref="br"/>
				<xs:element ref="pre"/>
				<xs:element ref="dl"/>
				<xs:element ref="ul"/>
				<xs:element ref="ol"/>
				<xs:element ref="crossref"/>
				<xs:element ref="figref"/>
				<xs:element ref="img"/>
				<xs:element ref="chemistry"/>
				<xs:element ref="maths"/>
				<xs:element ref="tables"/>
			</xs:choice>
		</xs:complexType>
	</xs:element>
	<xs:element name="claims">
		<xs:annotation>
			<xs:documentation>Claims of patent document.</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:choice>
				<xs:element ref="doc-page" maxOccurs="unbounded"/>
				<xs:sequence>
					<xs:element ref="claim-statement" minOccurs="0"/>
					<xs:element ref="claim" maxOccurs="unbounded"/>
				</xs:sequence>
			</xs:choice>
			<xs:attribute name="id" type="xs:ID"/>
			<xs:attribute name="lang" type="language-type"/>
			<xs:attribute name="format" type="format-type"/>
			<xs:attributeGroup ref="equivalent-source"/>
			<xs:attribute name="claim-type" type="xs:string"/>
			<xs:attribute name="status" type="xs:string"/>
			<xs:attribute name="date-changed" type="date-type"/>
		</xs:complexType>
	</xs:element>
	<xs:element name="claims-only-available">
		<xs:annotation>
			<xs:documentation>Document claim(s) only available.</xs:documentation>
			<xs:documentation>(ST.32 equivalency = B460)</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="date"/>
				<xs:element ref="text" minOccurs="0"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="class">
		<xs:annotation>
			<xs:documentation/>
		</xs:annotation>
		<xs:simpleType>
			<xs:restriction base="xs:string">
				<xs:pattern value="[0-9]{2}"/>
				<xs:pattern value="[A-Z0-9\.\-]+"/>
			</xs:restriction>
		</xs:simpleType>
	</xs:element>
	<xs:element name="classification">
		<xs:annotation>
			<xs:documentation>(US) Pair classification.</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="country"/>
				<xs:element name="main-classification" type="national-main-classification-type"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="classification-cpc">
		<xs:annotation>
			<xs:documentation>CPC classification.</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="text"/>
				<xs:sequence minOccurs="0">
					<xs:element ref="cpc-version-indicator"/>
					<xs:group ref="cpc-classification-group"/>
					<xs:element ref="symbol-position" minOccurs="0"/>
					<xs:element ref="classification-value" minOccurs="0"/>
					<xs:element ref="action-date" minOccurs="0"/>
					<xs:element ref="generating-office" minOccurs="0"/>
					<xs:element ref="classification-status" minOccurs="0"/>
					<xs:element ref="classification-data-source" minOccurs="0"/>
				</xs:sequence>
			</xs:sequence>
			<xs:attribute name="sequence" type="sequence-type"/>
		</xs:complexType>
	</xs:element>
	<xs:element name="classification-cpcno">
		<xs:annotation>
			<xs:documentation>CPCNO classification.</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="text"/>
				<xs:sequence minOccurs="0">
					<xs:element ref="cpc-version-indicator"/>
					<xs:group ref="cpc-classification-group"/>
					<xs:element ref="symbol-position" minOccurs="0"/>
					<xs:element ref="classification-value" minOccurs="0"/>
					<xs:element ref="action-date" minOccurs="0"/>
					<xs:element ref="generating-office" minOccurs="0"/>
					<xs:element ref="classification-status" minOccurs="0"/>
					<xs:element ref="classification-data-source" minOccurs="0"/>
				</xs:sequence>
			</xs:sequence>
			<xs:attribute name="sequence" type="sequence-type"/>
		</xs:complexType>
	</xs:element>
	<xs:element name="classification-data-source">
		<xs:annotation>
			<xs:documentation>IPC8 classification data source:</xs:documentation>
			<xs:documentation>H = human,</xs:documentation>
			<xs:documentation>M = machine generated and</xs:documentation>
			<xs:documentation>G = generated by software.</xs:documentation>
			<xs:documentation>CPC classification data source:</xs:documentation>
			<xs:documentation>H = human,</xs:documentation>
			<xs:documentation>M = machine generated and</xs:documentation>
			<xs:documentation>G = generated by software.</xs:documentation>
		</xs:annotation>
		<xs:simpleType>
			<xs:restriction base="xs:string">
				<xs:pattern value="[A-Z]"/>
			</xs:restriction>
		</xs:simpleType>
	</xs:element>
	<xs:element name="classification-ecla">
		<xs:annotation>
			<xs:documentation>European Patent Classification (ECLA) data.</xs:documentation>
			<xs:documentation>(ST.32 equivalency = B530)</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="text"/>
				<xs:sequence minOccurs="0">
					<xs:group ref="classification-group"/>
					<xs:element ref="additional-subgroups" minOccurs="0"/>
					<xs:element ref="examiner-edition" minOccurs="0"/>
				</xs:sequence>
			</xs:sequence>
			<xs:attribute name="sequence" type="sequence-type"/>
			<xs:attribute name="classification-scheme" type="classification-scheme-type" default="EC"/>
			<xs:attribute name="country" type="country-type" default="EP"/>
		</xs:complexType>
	</xs:element>
	<xs:element name="classification-f-term">
		<xs:annotation>
			<xs:documentation>Japanese F-terms classification.</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="text"/>
				<xs:sequence minOccurs="0">
					<xs:element name="theme" type="f-term-theme-type"/>
					<xs:element name="viewpoint" type="f-term-viewpoint-type"/>
					<xs:element name="figure" type="f-term-figure-type"/>
					<xs:element name="additional-code" type="f-term-additional-code-type" minOccurs="0"/>
				</xs:sequence>
			</xs:sequence>
			<xs:attribute name="sequence" type="sequence-type"/>
		</xs:complexType>
	</xs:element>
	<xs:element name="classification-ipc">
		<xs:annotation>
			<xs:documentation>International Patent Classification (IPC) data.</xs:documentation>
			<xs:documentation>(ST.32 equivalency = B510)</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element name="main-classification" type="ipc-main-classification-type"/>
				<xs:element name="further-classification" type="ipc-further-classification-type" minOccurs="0" maxOccurs="unbounded"/>
				<xs:group ref="classification-additional-group" minOccurs="0" maxOccurs="unbounded"/>
			</xs:sequence>
			<xs:attribute name="date-changed" type="date-type"/>
		</xs:complexType>
	</xs:element>
	<xs:element name="classification-ipc-original">
		<xs:annotation>
			<xs:documentation>Original International Patent Classification (IPC) data.</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element name="main-classification" type="ipc-main-classification-type"/>
				<xs:element name="further-classification" type="ipc-further-classification-type" minOccurs="0" maxOccurs="unbounded"/>
				<xs:group ref="classification-additional-group" minOccurs="0" maxOccurs="unbounded"/>
			</xs:sequence>
			<xs:attribute name="date-changed" type="date-type"/>
		</xs:complexType>
	</xs:element>
	<xs:element name="classification-ipcr">
		<xs:annotation>
			<xs:documentation>IPC8 classification.</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="text"/>
				<xs:sequence minOccurs="0">
					<xs:element ref="ipc-version-indicator"/>
					<xs:element ref="classification-level" minOccurs="0"/>
					<xs:group ref="classification-group"/>
					<xs:element ref="symbol-position" minOccurs="0"/>
					<xs:element ref="classification-value" minOccurs="0"/>
					<xs:element ref="action-date" minOccurs="0"/>
					<xs:element ref="generating-office" minOccurs="0"/>
					<xs:element ref="classification-status" minOccurs="0"/>
					<xs:element ref="classification-data-source" minOccurs="0"/>
				</xs:sequence>
			</xs:sequence>
			<xs:attribute name="sequence" type="sequence-type"/>
		</xs:complexType>
	</xs:element>
	<xs:element name="classification-level">
		<xs:annotation>
			<xs:documentation>IPC8 classification level, i.e. Core, Advanced or Subclass.</xs:documentation>
		</xs:annotation>
		<xs:simpleType>
			<xs:restriction base="xs:string">
				<xs:enumeration value="C"/>
				<xs:enumeration value="A"/>
				<xs:enumeration value="S"/>
			</xs:restriction>
		</xs:simpleType>
	</xs:element>
	<xs:element name="classification-locarno">
		<xs:annotation>
			<xs:documentation>For design patents only.</xs:documentation>
			<xs:documentation>Formerly inlcuded in IPC main classification.</xs:documentation>
			<xs:documentation>The Locarno Classification (LC) is formatted as fixed-length records as described here: Positions 1-2, "Class" (2 numeric, right justified, leading zeros); positions 3-4, "Subclass" (2 numeric, right justified, leading zeros).</xs:documentation>
			<xs:documentation>(ST.32 name: B511)</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="edition"/>
				<xs:element name="main-classification" type="locarno-classification-type"/>
				<xs:element name="further-classification" type="locarno-classification-type" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="text" minOccurs="0"/>
			</xs:sequence>
			<xs:attribute name="id" type="xs:ID"/>
			<xs:attribute name="date-changed" type="date-type"/>
		</xs:complexType>
	</xs:element>
	<xs:element name="classification-national">
		<xs:annotation>
			<xs:documentation>Domestic or national classification.</xs:documentation>
			<xs:documentation>(ST.32 equivalency = B520)</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="country"/>
				<xs:element name="main-classification" type="national-main-classification-type"/>
				<xs:element name="further-classification" type="national-further-classification-type" minOccurs="0" maxOccurs="unbounded"/>
				<xs:group ref="classification-additional-group" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="text" minOccurs="0"/>
			</xs:sequence>
			<xs:attribute name="id" type="xs:ID"/>
			<xs:attribute name="date-changed" type="date-type"/>
		</xs:complexType>
	</xs:element>
	<xs:element name="classification-national-original">
		<xs:annotation>
			<xs:documentation>Original domestic or national classification.</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="country"/>
				<xs:element name="main-classification" type="national-main-classification-type"/>
				<xs:element name="further-classification" type="national-further-classification-type" minOccurs="0" maxOccurs="unbounded"/>
				<xs:group ref="classification-additional-group" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="text" minOccurs="0"/>
			</xs:sequence>
			<xs:attribute name="id" type="xs:ID"/>
			<xs:attribute name="date-changed" type="date-type"/>
		</xs:complexType>
	</xs:element>
	<xs:element name="classification-status">
		<xs:annotation>
			<xs:documentation>IPC8 classification status:</xs:documentation>
			<xs:documentation>B = original,</xs:documentation>
			<xs:documentation>R = reclassified,</xs:documentation>
			<xs:documentation>V = various incidental changes and</xs:documentation>
			<xs:documentation>D = data to be deleted.</xs:documentation>
			<xs:documentation>CPC classification status:</xs:documentation>
			<xs:documentation>B = original and</xs:documentation>
			<xs:documentation>R = reclassified.</xs:documentation>
		</xs:annotation>
		<xs:simpleType>
			<xs:restriction base="xs:string">
				<xs:pattern value="[A-Z]"/>
			</xs:restriction>
		</xs:simpleType>
	</xs:element>
	<xs:element name="classification-value">
		<xs:annotation>
			<xs:documentation>IPC8 classification value:</xs:documentation>
			<xs:documentation>I = inventive and</xs:documentation>
			<xs:documentation>N = non-inventive.</xs:documentation>
			<xs:documentation>CPC classification value:</xs:documentation>
			<xs:documentation>I = inventive and</xs:documentation>
			<xs:documentation>A = additional.</xs:documentation>
		</xs:annotation>
		<xs:simpleType>
			<xs:restriction base="xs:string">
				<xs:pattern value="[A-Z]"/>
			</xs:restriction>
		</xs:simpleType>
	</xs:element>
	<xs:element name="classifications-cpc">
		<xs:annotation>
			<xs:documentation>CPC classifications.</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="classification-cpc" maxOccurs="unbounded"/>
				<xs:element ref="combination-set" minOccurs="0" maxOccurs="unbounded"/>
			</xs:sequence>
			<xs:attribute name="id" type="xs:ID"/>
			<xs:attribute name="date-changed" type="date-type"/>
		</xs:complexType>
	</xs:element>
	<xs:element name="classifications-cpcno">
		<xs:annotation>
			<xs:documentation>CPCNO classifications.</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="classification-cpcno" maxOccurs="unbounded"/>
				<xs:element ref="combination-set" minOccurs="0" maxOccurs="unbounded"/>
			</xs:sequence>
			<xs:attribute name="id" type="xs:ID"/>
			<xs:attribute name="date-changed" type="date-type"/>
		</xs:complexType>
	</xs:element>
	<xs:element name="classifications-ecla">
		<xs:annotation>
			<xs:documentation>ECLA classifications.</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="classification-ecla" maxOccurs="unbounded"/>
			</xs:sequence>
			<xs:attribute name="id" type="xs:ID"/>
			<xs:attribute name="date-changed" type="date-type"/>
		</xs:complexType>
	</xs:element>
	<xs:element name="classifications-f-term">
		<xs:annotation>
			<xs:documentation>Japanese F-terms classifications.</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="classification-f-term" maxOccurs="unbounded"/>
			</xs:sequence>
			<xs:attribute name="date-changed" type="date-type"/>
		</xs:complexType>
	</xs:element>
	<xs:element name="classifications-ipcr">
		<xs:annotation>
			<xs:documentation>IPC8 classifications.</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="classification-ipcr" maxOccurs="unbounded"/>
			</xs:sequence>
			<xs:attribute name="id" type="xs:ID"/>
			<xs:attribute name="date-changed" type="date-type"/>
		</xs:complexType>
	</xs:element>
	<xs:element name="classifications-ipcr-original">
		<xs:annotation>
			<xs:documentation>Original IPC8 classifications.</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="classification-ipcr" maxOccurs="unbounded"/>
			</xs:sequence>
			<xs:attribute name="id" type="xs:ID"/>
			<xs:attribute name="date-changed" type="date-type"/>
		</xs:complexType>
	</xs:element>
	<xs:element name="code">
		<xs:annotation>
			<xs:documentation>Classification FI code.</xs:documentation>
		</xs:annotation>
		<xs:simpleType>
			<xs:restriction base="xs:string">
				<xs:pattern value="[A-Z]{1}"/>
				<xs:pattern value="[ 0-9]{3}"/>
				<xs:pattern value="[ 0-9]{3} [ A-Z]"/>
			</xs:restriction>
		</xs:simpleType>
	</xs:element>
	<xs:element name="colf">
		<xs:annotation>
			<xs:documentation>Cited document column, first.</xs:documentation>
		</xs:annotation>
		<xs:simpleType>
			<xs:restriction base="text-type"/>
		</xs:simpleType>
	</xs:element>
	<xs:element name="coll">
		<xs:annotation>
			<xs:documentation>Cited document column, last.</xs:documentation>
		</xs:annotation>
		<xs:simpleType>
			<xs:restriction base="text-type"/>
		</xs:simpleType>
	</xs:element>
	<xs:element name="colspec">
		<xs:annotation>
			<xs:documentation>Table column specification.</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:attribute name="colnum" type="xs:NMTOKEN"/>
			<xs:attribute name="colname" type="xs:NMTOKEN"/>
			<xs:attribute name="colwidth" type="xs:string"/>
