	<!--  *  o  Optional element "classification-cpc" added to element "field-of-search".                          * -->
	<!--  *  o  Optional element "classification-cpc" added to element "citation".                                 * -->
	<!--  *  o  Added optional attribute "format" to element "drawings" to support equivalent clipped image.       * -->
	<!--  *  o  Added optional attribute-group "equivalent-source" to element "drawings" support equivalent.       * -->
	<!--  *  o  Added optional attribute "generated" to element "priority-claim" to support self-claimed priority. * -->
	<!--  *                                                                                                        * -->
	<!--  * September 2013 (version 1.12)                                                                          * -->
	<!--  *  o  Better support for "classification-ipcr" within element "field-of-search".                         * -->
	<!--  *  o  Added optional element "pair-data" to element "lexis-patent-document" to support US Pair data.     * -->
	<!--  *  o  Occurrence of mandatory element "document-id" changed to multiple under element "ep-publication".  * -->
	<!--  *  o  Added optional element "figure-drawings" to element "description".                                 * -->
	<!--  *  o  Added optional element "standardized-name" to element "assignee" to support standardized           * -->
	<!--           daily assignment names.                                                                         * -->
	<!--  *  o  Added optional element "normalized-name" to element "assignee" to support normalized               * -->
	<!--           daily assignment names.                                                                         * -->
	<!--  *  o  Added optional element "standardized-name" to group "name-group" to support standardized names.    * -->
	<!--  *  o  Added optional element "normalized-name" to group "name-group" to support normalized names.        * -->
	<!--  *  o  Added enumeration "lookup" to data-format-type to support looked up applicant names.               * -->
	<!--  *  o  Added enumeration "translated" to data-format-type to support translated applicant names.          * -->
	<!--  *  o  Added optional element "docdb-application-id" to element "legal-event".                            * -->
	<!--  *  o  Optional element "org-name" in group "name-group" changed to "orgname" according WIPO st.36.       * -->
	<!--  *  o  Removed pattern for element "document-code" as this element is being reused for US Pair data.      * -->
	<!--  *  o  Added optional element "combination-set" to element "classifications-cpc".                         * -->
	<!--  *  o  Added optional element "combination-set" to element "classifications-cpcno".                       * -->
	<!--  *  o  Added optional element "reinstatements" to element "post-issuance-data" to support                 * -->
	<!--           separating reinstatements from expirations in post-issuance data.                               * -->
	<!--  * March 2014 (version 1.12)                                                                              * -->
	<!--  *  o  Fix: Mandatory element "combination-set" under element "classifications-cpc" is now optional.      * -->
	<!--  *  o  Fix: Mandatory element "combination-set" under element "classifications-cpcno" is now optional.    * -->
	<!--  *                                                                                                        * -->
	<!--  * July 2014 (version 1.13)                                                                               * -->
	<!--  *  o Added optional element "priority-linkage-type" to element "prioriy-claim".                          * -->
	<!--  *  o Added optional element "priority-active-indicator" to element "prioriy-claim".                      * -->
	<!--  *  o Added optional element "claim-statement" to element "claims".                                       * -->
	<!--  *  o Added optional element "prime-notified-events" to element "lexisnexis-patent-document".             * -->
	<!--  *  o Removed pattern for element "edition" to support more than one digit.                               * -->
	<!--  *  o Added optional attribute "original" to element "img" for original (i.e. from source) filenames.     * -->
	<!--  *  o Added optional element "full-family" to element "patent-family" to support LNU full patent family.  * -->
	<!--  *  o Added optional attribute "main-family-id" to element "family-member" for family hierarchy.          * -->
	<!--  *  o Added optional attribute "complete-family-id" to element "family-member" for family hierarchy.      * -->
	<!--  *                                                                                                        * -->
	<!--  ********************************************************************************************************** -->
	<!-- xunx commented for CONT-3996
	<xs:import namespace="http://www.w3.org/XML/1998/namespace" schemaLocation="http://www.w3.org/2001/03/xml.xsd"/> -->
	<xs:import namespace="http://www.w3.org/XML/1998/namespace" schemaLocation="xocs-xml1998.xsd"/>
	<!--  ********** Schema Root Element **********   -->
	<xs:element name="lexisnexis-patent-document" type="lexisnexis-patent-document-type"/>
	<xs:complexType name="lexisnexis-patent-document-type">
		<xs:annotation>
			<xs:documentation>Top-level element of any patent document exported from the database according to WIPO st.36, USPTO st.36 and EPO st.36.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element ref="bibliographic-data"/>
			<xs:element ref="abstract" minOccurs="0" maxOccurs="unbounded"/>
			<xs:element ref="abstract-figure" minOccurs="0"/>
			<xs:element ref="front-page" minOccurs="0"/>
			<xs:element ref="legal-data" minOccurs="0"/>
			<xs:element ref="national-notifications" minOccurs="0"/>
			<xs:element ref="post-issuance-data" minOccurs="0" maxOccurs="unbounded"/>
			<xs:element ref="orangebook-data" minOccurs="0"/>
			<xs:element ref="pair-data" minOccurs="0"/>
			<xs:element ref="prime-notified-events" minOccurs="0"/>
			<xs:element ref="description" minOccurs="0" maxOccurs="unbounded"/>
			<xs:element ref="us-claim-statement" minOccurs="0"/>
			<xs:element ref="claims" minOccurs="0" maxOccurs="unbounded"/>
			<xs:element ref="amendments" minOccurs="0"/>
			<xs:element ref="drawings" minOccurs="0"/>
			<xs:element ref="image" minOccurs="0"/>
			<xs:element ref="sequence-list-doc" minOccurs="0"/>
			<xs:element ref="biological-material-doc" minOccurs="0"/>
		</xs:sequence>
		<xs:attribute name="lang" type="language-type" use="required"/>
		<xs:attribute name="schema-version" type="xs:string" use="required" fixed="1.13"/>
		<xs:attribute name="date-inserted" type="date-type"/>
		<xs:attribute name="time-inserted" type="time-type"/>
		<xs:attribute name="date-changed" type="date-type"/>
		<xs:attribute name="time-changed" type="time-type"/>
		<xs:attribute name="date-produced" type="date-type" use="required"/>
		<xs:attribute name="file" type="xs:string"/>
		<xs:attribute name="status" type="xs:string"/>
		<xs:attribute name="produced-by" type="xs:string" use="required"/>
	</xs:complexType>
	<!--  ********** Schema Elements (sorted from A-Z) ********** 
  -->
	<xs:element name="a-371-of-international">
		<xs:annotation>
			<xs:documentation>A 371 of international document.</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="relation"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="a-delays">
		<xs:annotation>
			<xs:documentation>(US) Pair A delays.</xs:documentation>
		</xs:annotation>
		<xs:simpleType>
			<xs:restriction base="text-type"/>
		</xs:simpleType>
	</xs:element>
	<xs:element name="absno">
		<xs:annotation>
			<xs:documentation>Abstract number.</xs:documentation>
		</xs:annotation>
		<xs:simpleType>
			<xs:restriction base="text-type"/>
		</xs:simpleType>
	</xs:element>
	<xs:element name="abst-problem">
		<xs:annotation>
			<xs:documentation>Abstract problem.</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="p" maxOccurs="unbounded"/>
			</xs:sequence>
			<xs:attribute name="id" type="xs:ID"/>
		</xs:complexType>
	</xs:element>
	<xs:element name="abst-solution">
		<xs:annotation>
			<xs:documentation>Abstract solution.</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="p" maxOccurs="unbounded"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="abstract">
		<xs:annotation>
			<xs:documentation>A concise summary of the disclosure, 50-150 words.</xs:documentation>
			<xs:documentation>(ST.32 equivalency = B570)</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:choice>
				<xs:element ref="doc-page" maxOccurs="unbounded"/>
				<xs:sequence>
					<xs:element ref="abst-problem"/>
					<xs:element ref="abst-solution"/>
				</xs:sequence>
				<xs:choice maxOccurs="unbounded">
					<xs:element ref="heading"/>
					<xs:element ref="p"/>
				</xs:choice>
			</xs:choice>
			<xs:attribute name="id" type="xs:ID"/>
			<xs:attribute name="lang" type="language-type"/>
			<xs:attribute name="format" type="format-type"/>
			<xs:attributeGroup ref="equivalent-source"/>
			<xs:attribute name="status" type="xs:string"/>
			<xs:attribute name="date-changed" type="date-type"/>
			<xs:attribute name="language" use="optional" type="xs:string"/>
		</xs:complexType>
	</xs:element>
	<xs:element name="abstract-figure">
		<xs:annotation>
			<xs:documentation>Note: the abstract figure (if there is one) may be the actual image, or simply a reference to the figure chosen by the applicant or the office.</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:choice>
				<xs:element ref="fig-number" maxOccurs="unbounded"/>
				<xs:element ref="img" maxOccurs="unbounded"/>
			</xs:choice>
			<xs:attribute name="id" type="xs:ID"/>
			<xs:attribute name="fig-status" type="figure-status-type" default="1"/>
			<xs:attribute name="orientation" type="xs:string"/>
			<xs:attribute name="lang" type="xs:string"/>
		</xs:complexType>
	</xs:element>
	<xs:element name="abstract-reference">
		<xs:annotation>
			<xs:documentation>Date of separate publication of an abstract.</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="date"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="accepted">
		<xs:annotation>
			<xs:documentation>Date article was accepted.</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="date"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="action-date">
		<xs:annotation>
			<xs:documentation>IPC8 action date.</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="date"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="addition">
		<xs:annotation>
			<xs:documentation>Earlier document to which this is an addition.</xs:documentation>
			<xs:documentation>(ST.32 equivalency = B610)</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="relation"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="additional-indication">
		<xs:annotation>
			<xs:documentation>Additional indication information for the deposit.</xs:documentation>
		</xs:annotation>
		<xs:simpleType>
			<xs:restriction base="text-type"/>
		</xs:simpleType>
	</xs:element>
	<xs:element name="additional-info">
		<xs:annotation>
			<xs:documentation>Additional information.</xs:documentation>
			<xs:documentation>(ST.32 equivalency = B513)</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:group ref="ipc-classification-group"/>
			</xs:sequence>
			<xs:attribute name="sequence" type="sequence-type"/>
		</xs:complexType>
	</xs:element>
	<xs:element name="additional-subgroup">
		<xs:annotation>
			<xs:documentation>European Patent Classification (ECLA) additional subgroup.</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:simpleContent>
				<xs:extension base="text-type">
					<xs:attribute name="id" type="xs:ID"/>
					<xs:attribute name="sequence" type="sequence-type"/>
				</xs:extension>
			</xs:simpleContent>
		</xs:complexType>
	</xs:element>
	<xs:element name="additional-subgroups">
		<xs:annotation>
			<xs:documentation>European Patent Classification (ECLA) additional subgroups.</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="additional-subgroup" maxOccurs="unbounded"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="address">
		<xs:annotation>
			<xs:documentation>Address information.</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:choice>
				<xs:sequence>
					<xs:element ref="address-1" minOccurs="0"/>
					<xs:element ref="address-2" minOccurs="0"/>
					<xs:element ref="address-3" minOccurs="0"/>
					<xs:element ref="address-4" minOccurs="0"/>
					<xs:element ref="address-5" minOccurs="0"/>
					<xs:element ref="mailcode" minOccurs="0"/>
					<xs:element ref="pobox" minOccurs="0"/>
					<xs:element ref="room" minOccurs="0"/>
					<xs:element ref="address-floor" minOccurs="0"/>
					<xs:element ref="building" minOccurs="0"/>
					<xs:element ref="street" minOccurs="0"/>
					<xs:element ref="city" minOccurs="0"/>
					<xs:element ref="county" minOccurs="0"/>
					<xs:element ref="state" minOccurs="0"/>
					<xs:element ref="postcode" minOccurs="0"/>
					<xs:element ref="country" minOccurs="0"/>
				</xs:sequence>
				<xs:element ref="text"/>
			</xs:choice>
		</xs:complexType>
	</xs:element>
	<xs:element name="address-1">
		<xs:annotation>
			<xs:documentation>Address, 1st line.</xs:documentation>
		</xs:annotation>
		<xs:simpleType>
			<xs:restriction base="text-type"/>
		</xs:simpleType>
	</xs:element>
	<xs:element name="address-2">
		<xs:annotation>
			<xs:documentation>Address, 2nd line.</xs:documentation>
		</xs:annotation>
		<xs:simpleType>
			<xs:restriction base="text-type"/>
		</xs:simpleType>
	</xs:element>
	<xs:element name="address-3">
		<xs:annotation>
			<xs:documentation>Address, 3rd line.</xs:documentation>
		</xs:annotation>
		<xs:simpleType>
			<xs:restriction base="text-type"/>
		</xs:simpleType>
	</xs:element>
	<xs:element name="address-4">
		<xs:annotation>
			<xs:documentation>Address, 4th line.</xs:documentation>
		</xs:annotation>
		<xs:simpleType>
			<xs:restriction base="text-type"/>
		</xs:simpleType>
	</xs:element>
	<xs:element name="address-5">
		<xs:annotation>
			<xs:documentation>Address, 5th line.</xs:documentation>
		</xs:annotation>
		<xs:simpleType>
			<xs:restriction base="text-type"/>
		</xs:simpleType>
	</xs:element>
	<xs:element name="address-and-attorney-agent">
		<xs:annotation>
			<xs:documentation>(US) Pair address and attorneys/agents.</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="correspondence-address"/>
				<xs:element ref="attorney-agent" minOccurs="0" maxOccurs="unbounded"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="address-floor">
		<xs:annotation>
			<xs:documentation>Address floor, e.g., 10th.</xs:documentation>
		</xs:annotation>
		<xs:simpleType>
			<xs:restriction base="text-type"/>
		</xs:simpleType>
	</xs:element>
	<xs:element name="addressbook">
		<xs:annotation>
			<xs:documentation>ADDRESSBOOK GROUP: Holds name and contact information for individuals/organizations.</xs:documentation>
			<xs:documentation>The LANG attribute is REQUIRED for certain document types, e.g. the JPO PCT Request where names and addresses may be repeated in two different languages.</xs:documentation>
			<xs:documentation>If no structure possible use text.</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:choice>
				<xs:sequence>
					<xs:group ref="name-group"/>
					<xs:element ref="address" minOccurs="0"/>
					<xs:element ref="phone" minOccurs="0"/>
					<xs:element ref="fax" minOccurs="0"/>
					<xs:element ref="email" minOccurs="0"/>
					<xs:element ref="url" minOccurs="0"/>
					<xs:element ref="ead" minOccurs="0"/>
					<xs:element ref="dtext" minOccurs="0"/>
				</xs:sequence>
				<xs:element ref="text"/>
			</xs:choice>
			<xs:attribute name="id" type="xs:ID"/>
			<xs:attribute name="lang" type="language-type"/>
		</xs:complexType>
	</xs:element>
	<xs:element name="advantageous-effects">
		<xs:annotation>
			<xs:documentation>Advantageous effects of the invention.</xs:documentation>
			<xs:documentation>PCT Rule 5.1(a)(iii)</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence maxOccurs="unbounded">
				<xs:element ref="heading" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="p" maxOccurs="unbounded"/>
			</xs:sequence>
			<xs:attribute name="id" type="xs:ID"/>
		</xs:complexType>
	</xs:element>
	<xs:element name="adverse-decisions">
		<xs:annotation>
			<xs:documentation>US Post-issuance adverse decisions.</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element name="adverse-decision" type="post-issuance-element-type" maxOccurs="unbounded"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="agent">
		<xs:annotation>
			<xs:documentation>Agents and attorneys.</xs:documentation>
			<xs:documentation>(ST.32 equivalency = B741)</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="addressbook" maxOccurs="unbounded"/>
			</xs:sequence>
			<xs:attribute name="sequence" type="sequence-type" use="required"/>
			<xs:attribute name="rep-type" type="representative-type" use="required"/>
			<xs:attribute name="data-format" type="data-format-type"/>
		</xs:complexType>
	</xs:element>
	<xs:element name="agents">
		<xs:annotation>
			<xs:documentation>Information regarding Agents or common representatives.</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:choice>
				<xs:element ref="customer-number"/>
				<xs:element ref="agent" maxOccurs="unbounded"/>
			</xs:choice>
		</xs:complexType>
	</xs:element>
	<xs:element name="aia-first-inventor-to-file">
		<xs:annotation>
			<xs:documentation>(US) Pair AIA first inventor to file.</xs:documentation>
		</xs:annotation>
		<xs:simpleType>
			<xs:restriction base="xs:boolean"/>
		</xs:simpleType>
	</xs:element>
	<xs:element name="all-designations">
		<xs:annotation>
			<xs:documentation>All designations permitted by the Treaty are made.</xs:documentation>
			<xs:documentation>Rule 4.9(b)</xs:documentation>
		</xs:annotation>
		<xs:simpleType>
			<xs:restriction base="xs:string">
				<xs:length value="0"/>
			</xs:restriction>
		</xs:simpleType>
	</xs:element>
	<xs:element name="alttitle">
		<xs:annotation>
			<xs:documentation>Alternate title for serial; e.g. BMJ = British Medical Journal.</xs:documentation>
		</xs:annotation>
		<xs:simpleType>
			<xs:restriction base="text-type"/>
		</xs:simpleType>
	</xs:element>
	<xs:element name="amendments">
		<xs:annotation>
			<xs:documentation>Amendments to the patent document.</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="amendment" maxOccurs="unbounded"/>
			</xs:sequence>
			<xs:attribute name="date-changed" type="date-type"/>
		</xs:complexType>
	</xs:element>
	<xs:element name="amendment">
		<xs:annotation>
			<xs:documentation>Amendment to the patent document.</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="amendment-date" minOccurs="0"/>
				<xs:element ref="amendment-group" maxOccurs="unbounded"/>
			</xs:sequence>
			<xs:attribute name="num" type="xs:positiveInteger" use="required"/>
			<xs:attribute name="type" type="amendment-type" use="required"/>
		</xs:complexType>
	</xs:element>
	<xs:element name="amendment-content">
		<xs:annotation>
			<xs:documentation>Contents of the amendment.</xs:documentation>
		</xs:annotation>
		<xs:complexType mixed="true">
			<xs:choice maxOccurs="unbounded">
				<xs:any processContents="skip"/>
			</xs:choice>
			<xs:attribute name="law-kind" type="content-law-kind-type"/>
			<xs:attribute name="doc-kind" type="content-doc-kind-type"/>
		</xs:complexType>
	</xs:element>
	<xs:element name="amendment-date">
		<xs:annotation>
			<xs:documentation>Date of the amendment.</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="date"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="amendment-group">
		<xs:annotation>
			<xs:documentation>Amendment-group of an amendment.</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:all>
				<xs:element ref="purpose-of-statement" minOccurs="0"/>
				<xs:element ref="deleted-claims" minOccurs="0"/>
				<xs:element ref="number-of-amended-claims" minOccurs="0"/>
				<xs:element ref="document-code" minOccurs="0"/>
				<xs:element ref="amendment-item" minOccurs="0"/>
				<xs:element ref="amendment-way" minOccurs="0"/>
				<xs:element ref="amendment-content" minOccurs="0"/>
			</xs:all>
			<xs:attribute name="num" type="xs:positiveInteger" use="required"/>
			<xs:attribute name="lang" type="language-type"/>
		</xs:complexType>
	</xs:element>
	<xs:element name="amendment-item">
		<xs:annotation>
			<xs:documentation>Item of the amendment.</xs:documentation>
		</xs:annotation>
		<xs:simpleType>
			<xs:restriction base="text-type"/>
		</xs:simpleType>
	</xs:element>
	<xs:element name="amendment-way">
		<xs:annotation>
			<xs:documentation>Way of the amendment.</xs:documentation>
		</xs:annotation>
		<xs:simpleType>
			<xs:restriction base="xs:string">
				<xs:pattern value="[1-9][0-9]*"/>
			</xs:restriction>
		</xs:simpleType>
	</xs:element>
	<xs:element name="annual-serial-number">
		<xs:annotation>
			<xs:documentation>Annual serial number.</xs:documentation>
		</xs:annotation>
		<xs:simpleType>
			<xs:restriction base="xs:string">
				<xs:pattern value="[0-9]+"/>
			</xs:restriction>
		</xs:simpleType>
	</xs:element>
	<xs:element name="any-other-state">
		<xs:annotation>
			<xs:documentation>Any other state in which protection is being sought.</xs:documentation>
		</xs:annotation>
		<xs:complexType/>
	</xs:element>
	<xs:element name="applicant">
		<xs:annotation>
			<xs:documentation>(US: applicant is always the inventor. Use "applicant" with applicant type attribute of "applicant-inventor".)</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
