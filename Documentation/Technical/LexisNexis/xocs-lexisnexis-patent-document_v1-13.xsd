			<xs:attribute name="name" type="text-type"/>
		</xs:complexType>
	</xs:element>
	<xs:element name="printed-as-amended">
		<xs:annotation>
			<xs:documentation>Document printed as amended, eg. EPO B2.</xs:documentation>
