			<xs:sequence>
				<xs:element ref="date"/>
				<xs:element ref="text" minOccurs="0"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="printed-with-grant">
		<xs:annotation>
			<xs:documentation>Date of publication by printing or similar process of document on which grant has taken place on or before the said date.</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="date"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="priority-active-indicator">
		<xs:annotation>
			<xs:documentation>Priority active indicator.</xs:documentation>
