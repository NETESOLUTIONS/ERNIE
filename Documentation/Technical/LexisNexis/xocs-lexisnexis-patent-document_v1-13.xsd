			<xs:restriction base="xs:boolean"/>
		</xs:simpleType>
	</xs:element>
	<xs:element name="priority-application">
		<xs:annotation>
			<xs:documentation>Priority application number.</xs:documentation>
			<xs:documentation>(INID 31, ST.32 equivalency = B310)</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="document-id"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="priority-claim">
		<xs:annotation>
			<xs:documentation>Priority claim.</xs:documentation>
			<xs:documentation>(ST.32 equivalency = B300)</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="country" minOccurs="0"/>
				<xs:element ref="doc-number"/>
				<xs:element ref="kind" minOccurs="0"/>
				<xs:element ref="date" minOccurs="0"/>
				<xs:element ref="office-of-filing" minOccurs="0"/>
				<xs:choice minOccurs="0">
					<xs:element ref="priority-doc-requested"/>
					<xs:element ref="priority-doc-attached"/>
				</xs:choice>
				<xs:element ref="priority-linkage-type" minOccurs="0"/>
				<xs:element ref="priority-active-indicator" minOccurs="0"/>
			</xs:sequence>
			<xs:attribute name="id" type="xs:ID"/>
			<xs:attribute name="sequence" type="sequence-type" use="required"/>
			<xs:attribute name="kind" type="priority-kind"/>
			<xs:attribute name="data-format" type="data-format-type"/>
			<xs:attribute name="generated" type="xs:boolean"/>
		</xs:complexType>
	</xs:element>
	<xs:element name="priority-claim-ib-info">
		<xs:annotation>
			<xs:documentation>Additional priority claim information by IB.</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="received-at-ib" minOccurs="0"/>
				<xs:element ref="pct-rule17-1" minOccurs="0"/>
				<xs:element ref="withdrawn" minOccurs="0"/>
				<xs:element ref="considered-not-made" minOccurs="0"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="priority-claims">
		<xs:annotation>
			<xs:documentation>Applications in which priority is claimed.</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence maxOccurs="unbounded">
				<xs:element ref="priority-claim"/>
				<xs:element ref="priority-claim-ib-info" minOccurs="0"/>
			</xs:sequence>
			<xs:attribute name="date-changed" type="date-type"/>
		</xs:complexType>
	</xs:element>
	<xs:element name="priority-doc-attached">
		<xs:annotation>
			<xs:documentation>The priority document is attached to the application.</xs:documentation>
		</xs:annotation>
		<xs:complexType/>
	</xs:element>
	<xs:element name="priority-doc-requested">
		<xs:annotation>
			<xs:documentation>The receiving office is requested to prepare and transmit to the IB a certified copy of the earlier application.</xs:documentation>
		</xs:annotation>
		<xs:complexType/>
	</xs:element>
	<xs:element name="priority-linkage-type">
		<xs:annotation>
			<xs:documentation>Priority linkage type.</xs:documentation>
			<xs:documentation>Indication of priority relation, eg. '1' = "for continuation"</xs:documentation>
		</xs:annotation>
		<xs:simpleType>
			<xs:restriction base="xs:string">
				<xs:pattern value="[A-Z0-9]"/>
			</xs:restriction>
		</xs:simpleType>
	</xs:element>
	<xs:element name="proceedings-interrupted">
		<xs:annotation>
			<xs:documentation>Date of interruption of proceedings.</xs:documentation>
			<xs:documentation>(ST.32 equivalency = B245)</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="date"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="proceedings-resumed">
		<xs:annotation>
			<xs:documentation>Date of resumption of proceedings.</xs:documentation>
			<xs:documentation>(ST.32 equivalency = B246)</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="date"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="proceedings-suspended">
		<xs:annotation>
			<xs:documentation>Date of suspension of proceedings.</xs:documentation>
			<xs:documentation>(ST.32 equivalency = B245)</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="date"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="product">
		<xs:annotation>
			<xs:documentation>OrangeBook Approved Drug Product.</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element name="ingredient" type="text-type" maxOccurs="unbounded"/>
				<xs:element name="dosage-form" type="text-type" minOccurs="0"/>
				<xs:element name="route" type="text-type" minOccurs="0"/>
				<xs:element name="proprietary-name" type="text-type" minOccurs="0"/>
				<xs:element name="applicant-name" type="text-type" minOccurs="0"/>
				<xs:element name="applicant-fullname" type="text-type" minOccurs="0"/>
				<xs:element name="strength" type="text-type" minOccurs="0"/>
				<xs:element ref="therapeutic-equivalence" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="approval-date" minOccurs="0"/>
			</xs:sequence>
			<xs:attribute name="product-type" type="text-type"/>
			<xs:attribute name="reference-listed-drug" type="boolean-type"/>
		</xs:complexType>
	</xs:element>
	<xs:element name="protection-request">
		<xs:annotation>
			<xs:documentation>Protection request for the designated office.</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="kind-of-protection"/>
				<xs:element ref="document-id" minOccurs="0"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="provisional-application">
		<xs:annotation>
			<xs:documentation>Provisional application that is the basis of the document in hand.</xs:documentation>
			<xs:documentation>(ST.32 equivalency = B680US)</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="document-id" maxOccurs="unbounded"/>
				<xs:element ref="provisional-application-status" minOccurs="0"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="provisional-application-status">
		<xs:annotation>
			<xs:documentation>Status of the provisional application that is the basis of the document in hand.</xs:documentation>
		</xs:annotation>
		<xs:simpleType>
			<xs:restriction base="xs:string">
				<xs:minLength value="1"/>
			</xs:restriction>
		</xs:simpleType>
	</xs:element>
	<xs:element name="pto-manual-adjustments">
		<xs:annotation>
			<xs:documentation>(US) Pair PTO manual adjustments.</xs:documentation>
		</xs:annotation>
		<xs:simpleType>
			<xs:restriction base="text-type"/>
		</xs:simpleType>
	</xs:element>
	<xs:element name="pubdate">
		<xs:annotation>
			<xs:documentation>Publication date used in npl citations.</xs:documentation>
		</xs:annotation>
		<xs:complexType mixed="true">
			<xs:choice minOccurs="0" maxOccurs="unbounded">
				<xs:element ref="sdate"/>
				<xs:element ref="edate"/>
				<xs:element ref="time"/>
			</xs:choice>
		</xs:complexType>
	</xs:element>
	<xs:element name="pubid">
		<xs:annotation>
			<xs:documentation>Publisher's unique id.</xs:documentation>
		</xs:annotation>
		<xs:simpleType>
			<xs:restriction base="text-type"/>
		</xs:simpleType>
	</xs:element>
	<xs:element name="publication">
		<xs:annotation>
			<xs:documentation>(US) Pair patent number.</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="document-id" maxOccurs="unbounded"/>
			</xs:sequence>
			<xs:attribute name="id" type="xs:ID"/>
		</xs:complexType>
	</xs:element>
	<xs:element name="publication-date">
		<xs:annotation>
			<xs:documentation>Legal publication date.</xs:documentation>
			<xs:documentation>(ST.32 equivalency = L007EP)</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="date"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="publication-of-grant-date">
		<xs:annotation>
			<xs:documentation>Publication date of granted document.</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="date"/>
				<xs:element ref="text" minOccurs="0"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="publication-reference">
		<xs:annotation>
			<xs:documentation>Publication reference information: publication number, date, etc.</xs:documentation>
			<xs:documentation>(ST.32 equivalency = B110)</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="document-id" maxOccurs="unbounded"/>
			</xs:sequence>
			<xs:attribute name="id" type="xs:ID"/>
			<xs:attribute name="publ-type" type="text-type"/>
			<xs:attribute name="publ-desc" type="text-type"/>
		</xs:complexType>
	</xs:element>
	<xs:element name="purpose-of-statement">
		<xs:annotation>
			<xs:documentation>Purpose of amendment.</xs:documentation>
		</xs:annotation>
		<xs:simpleType>
			<xs:restriction base="text-type"/>
		</xs:simpleType>
	</xs:element>
	<xs:element name="qualifying-character">
		<xs:annotation>
			<xs:documentation>IPC classification qualifying character.</xs:documentation>
		</xs:annotation>
		<xs:simpleType>
			<xs:restriction base="xs:string">
				<xs:pattern value="[ A-CJ-Z\-]"/>
			</xs:restriction>
		</xs:simpleType>
	</xs:element>
	<xs:element name="rank-number">
		<xs:annotation>
			<xs:documentation>The sequential number that is used to identify the rank of a symbol within a combination set. (Order of the symbols is important).</xs:documentation>
		</xs:annotation>
		<xs:simpleType>
			<xs:restriction base="text-type"/>
		</xs:simpleType>
	</xs:element>
	<xs:element name="received">
		<xs:annotation>
			<xs:documentation>Date article was received.</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="date"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="received-at-ib">
		<xs:annotation>
			<xs:documentation>International Bureau receipt date.</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="date"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="recorded-date">
		<xs:annotation>
			<xs:documentation>Post-issuance recorded date.</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="date"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="reel-frame-number">
		<xs:annotation>
			<xs:documentation>Post-issuance reel-frame number.</xs:documentation>
		</xs:annotation>
		<xs:simpleType>
			<xs:restriction base="text-type"/>
		</xs:simpleType>
	</xs:element>
	<xs:element name="reexamination">
		<xs:annotation>
			<xs:documentation>Document of which this is a reexamination.</xs:documentation>
			<xs:documentation>(ST.32 equivalency = B645)</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="relation"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="reexaminations">
		<xs:annotation>
			<xs:documentation>Post-issuance reexaminations.</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element name="reexamination" type="post-issuance-element-type" maxOccurs="unbounded"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="reexamination-reissue-merger">
		<xs:annotation>
			<xs:documentation>Present when a reissue application was merged with a reexamination proceeding. The issuing document is a reissue patent which shows the following Reexamination results statement on the reissue patent front page, immediately following the abstract. NNNNN represents the Reexamination Request Number, and YYYYMMDD the filing date. "The questions raised in reexamination request No.90/NNNNN, filed YYYYMMDD, have been considered and the results thereof are reflected in this reissue patent which constitutes the reexamination certificate required by 35 USC 307 as provided in 37 CFR 1.570(e)." Document (parent) under rexeamination that merged with the reissue proceedings from which the document in hand (child) resulted.</xs:documentation>
			<xs:documentation>(ST.32 equivalency = B645US)</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="relation"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="reference-signs-list">
		<xs:annotation>
			<xs:documentation>Reference signs list.</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence maxOccurs="unbounded">
				<xs:element ref="heading" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="p" maxOccurs="unbounded"/>
			</xs:sequence>
			<xs:attribute name="id" type="xs:ID"/>
		</xs:complexType>
	</xs:element>
	<xs:element name="reference-to-deposited-biological-material">
		<xs:annotation>
			<xs:documentation>Reference to deposited biological material.</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence maxOccurs="unbounded">
				<xs:element ref="heading" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="p" maxOccurs="unbounded"/>
			</xs:sequence>
			<xs:attribute name="id" type="xs:ID"/>
		</xs:complexType>
	</xs:element>
	<xs:element name="references-cited">
		<xs:annotation>
			<xs:documentation>This tag and elements to be used primary to code data on the title page of patents under INID code (56). e.g. US patent. For the search report as a sub-document use search-report, e.g. for ISR.</xs:documentation>
			<xs:documentation>(ST.32 equivalency = B560)</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:choice>
				<xs:element ref="text"/>
				<xs:sequence>
					<xs:element ref="patent-citations" minOccurs="0"/>
					<xs:element ref="non-patent-citations" minOccurs="0"/>
					<xs:element ref="forward-citations" minOccurs="0"/>
					<xs:element ref="citation" maxOccurs="unbounded"/>
					<xs:element ref="date-search-completed" minOccurs="0"/>
					<xs:element ref="date-search-report-mailed" minOccurs="0"/>
					<xs:element ref="place-of-search" minOccurs="0"/>
					<xs:element ref="search-report-publication" minOccurs="0"/>
					<xs:element ref="searcher" minOccurs="0"/>
				</xs:sequence>
			</xs:choice>
			<xs:attribute name="date-changed" type="date-type"/>
		</xs:complexType>
	</xs:element>
	<xs:element name="refno">
		<xs:annotation>
			<xs:documentation>Reference number (general); for the EPO this is the XP number.</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:simpleContent>
				<xs:extension base="xs:string">
					<xs:attribute name="id" type="xs:ID"/>
				</xs:extension>
			</xs:simpleContent>
		</xs:complexType>
	</xs:element>
	<xs:element name="region">
		<xs:annotation>
			<xs:documentation>The region for which protection or exclusion (depending upon context) is desired.</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="country"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="regional">
		<xs:annotation>
			<xs:documentation>Regional (as opposed to national) protection being sought.</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="region"/>
				<xs:choice maxOccurs="unbounded">
					<xs:element ref="country"/>
					<xs:element ref="protection-request" minOccurs="0" maxOccurs="unbounded"/>
				</xs:choice>
				<xs:element ref="any-other-state" minOccurs="0"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="registered-number">
		<xs:annotation>
			<xs:documentation>Registered number, format undefined.</xs:documentation>
		</xs:annotation>
		<xs:simpleType>
			<xs:restriction base="text-type"/>
		</xs:simpleType>
	</xs:element>
	<xs:element name="reinstatements">
		<xs:annotation>
			<xs:documentation>Post-issuance reinstatement.</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element name="reinstatement" type="post-issuance-element-type" maxOccurs="unbounded"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="reissue">
		<xs:annotation>
			<xs:documentation>Document being reissued.</xs:documentation>
			<xs:documentation>(ST.32 equivalency = B640)</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="relation"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="reissues">
		<xs:annotation>
			<xs:documentation>Post-issuance reissues.</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element name="reissue" type="post-issuance-element-type" maxOccurs="unbounded"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="rel-claims">
		<xs:annotation>
			<xs:documentation>Relevant claims.</xs:documentation>
		</xs:annotation>
		<xs:simpleType>
			<xs:restriction base="text-type"/>
		</xs:simpleType>
	</xs:element>
	<xs:element name="rel-passage">
		<xs:annotation>
			<xs:documentation>Relevant passage group within patent; most used in search reports - use text in abstract, description and claims.</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:choice>
				<xs:element ref="text"/>
				<xs:sequence maxOccurs="unbounded">
					<xs:element ref="passage" maxOccurs="unbounded"/>
					<xs:element ref="category" minOccurs="0" maxOccurs="unbounded"/>
					<xs:element ref="rel-claims" minOccurs="0" maxOccurs="unbounded"/>
				</xs:sequence>
			</xs:choice>
		</xs:complexType>
	</xs:element>
	<xs:element name="related-apps">
		<xs:annotation>
			<xs:documentation>Cross-reference to related applications.</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence maxOccurs="unbounded">
				<xs:element ref="heading" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="p" maxOccurs="unbounded"/>
			</xs:sequence>
			<xs:attribute name="id" type="xs:ID"/>
		</xs:complexType>
	</xs:element>
	<xs:element name="related-documents">
		<xs:annotation>
			<xs:documentation>Various relationships between the patent in hand and other patent grants or applications. Contains either an additional application, a divisional application, continuations, reissues, divisional reissues, reexamination, merged reissues reexamination, substitute, or provisional application.</xs:documentation>
			<xs:documentation>(ST.32 equivalency = B600)</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:choice maxOccurs="unbounded">
				<xs:element ref="addition"/>
				<xs:element ref="division"/>
				<xs:element ref="continuation"/>
				<xs:element ref="continuation-in-part"/>
				<xs:element ref="continuing-reissue"/>
				<xs:element ref="reissue"/>
				<xs:element ref="divisional-reissue"/>
				<xs:element ref="reexamination"/>
				<xs:element ref="reexamination-reissue-merger"/>
				<xs:element ref="substitution"/>
				<xs:element ref="provisional-application"/>
				<xs:element ref="utility-model-basis"/>
				<xs:element ref="correction"/>
				<xs:element ref="related-publication"/>
				<xs:element ref="a-371-of-international"/>
			</xs:choice>
			<xs:attribute name="date-changed" type="date-type"/>
		</xs:complexType>
	</xs:element>
	<xs:element name="related-publication">
		<xs:annotation>
			<xs:documentation>Previously published document concerning same application.</xs:documentation>
			<xs:documentation>(ST.32 equivalency = B650)</xs:documentation>
		</xs:annotation>
		<xs:complexType mixed="true">
			<xs:sequence>
				<xs:element ref="document-id" minOccurs="0" maxOccurs="unbounded"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="relation">
		<xs:annotation>
			<xs:documentation>If there is more than one divisional reissue, siblings are indicated by multiple occurences of child.</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="parent-doc"/>
				<xs:element ref="child-doc"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="representative-name">
		<xs:annotation>
			<xs:documentation>Legal representative name.</xs:documentation>
			<xs:documentation>(ST.32 equivalency = L517EP)</xs:documentation>
		</xs:annotation>
		<xs:simpleType>
			<xs:restriction base="text-type"/>
		</xs:simpleType>
	</xs:element>
	<xs:element name="request-for-conversion-to-national-application">
