<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" elementFormDefault="qualified" attributeFormDefault="unqualified" targetNamespace="http://www.elsevier.com/xml/xocs/dtd" xmlns:ait="http://www.elsevier.com/xml/ani/ait"
           xmlns:ani="http://www.elsevier.com/xml/ani/ani" xmlns:xocs="http://www.elsevier.com/xml/xocs/dtd" xmlns:cto="http://www.elsevier.com/xml/cto/dtd" xmlns:ce="http://www.elsevier.com/xml/ani/common">

	<xs:import namespace="http://www.elsevier.com/xml/ani/ait" schemaLocation="xocs-ani513-ait.xsd"/>
	<xs:import namespace="http://www.elsevier.com/xml/ani/common" schemaLocation="xocs-ani513-ce.xsd"/>
	<xs:import namespace="http://www.w3.org/XML/1998/namespace" schemaLocation="xocs-ani513-xml.xsd"/>
	<xs:import namespace="http://www.elsevier.com/xml/ani/ani" schemaLocation="xocs-ani513-ani-ce.xsd"/>
	<xs:import namespace="http://www.elsevier.com/xml/cto/dtd" schemaLocation="xocs-ani513-cto.xsd"/>

	<xs:element name="doc" type="xocs:doc-tp"/>

	<xs:complexType name="doc-tp">
		<xs:sequence>
			<xs:element name="meta" type="xocs:meta-tp"/>
			<xs:element name="item" type="xocs:item-tp"/>
			<!-- xs:element ref="xocs:item"/ -->
		</xs:sequence>
		<xs:attribute name="content-type" type="xocs:content-type-tp" use="required"/>
		<xs:attribute name="dbname" type="xocs:dbname-tp" use="required"/>
	</xs:complexType>

	<xs:simpleType name="content-type-tp">
		<xs:restriction base="xs:string">
			<xs:enumeration value="Abstract"/>
			<xs:enumeration value="journal"/>
		</xs:restriction>
	</xs:simpleType>

	<xs:simpleType name="dbname-tp">
		<xs:restriction base="xs:string">
			<xs:enumeration value="scopusbase"/>
			<xs:enumeration value="econlit"/>
			<xs:enumeration value="psycinfo"/>
			<xs:enumeration value="medline"/>
		</xs:restriction>
	</xs:simpleType>

	<xs:complexType name="meta-tp">
		<xs:sequence>
			<xs:element name="eid" type="xocs:nonEmptyString" nillable="false"/>
			<xs:element name="oeid" type="xs:string"/>
			<xs:element name="doi" type="xs:string" minOccurs="0" maxOccurs="1"/>
			<xs:element name="pmid" type="xs:string" minOccurs="0" maxOccurs="1"/>
			<xs:element name="online-status" type="xocs:online-status-tp"/>
			<xs:element name="pui" type="xs:string" minOccurs="0" maxOccurs="1"/>
			<xs:element name="pii" type="xs:string" minOccurs="0" maxOccurs="1"/>
			<xs:element name="fulltext-eid" type="xocs:fulltext-eid-tp" minOccurs="0" maxOccurs="1"/>
			<xs:element name="sdfullavail" type="xs:integer"/>
			<xs:element name="issn" type="xs:string"/>
			<xs:element name="volume" type="xs:string"/>
			<xs:element name="issue" type="xs:string"/>
			<xs:element name="author-surname" type="xs:string"/>
			<xs:element name="author-fi" type="xs:string"/>
			<xs:element name="firstpage" type="xs:string"/>
			<xs:element name="lastpage" type="xs:string"/>
			<xs:element name="sort-year" type="xs:string"/>
			<xs:element name="sort-yyyymm" type="xs:string"/>
			<xs:element name="pub-year" type="xs:string" minOccurs="0" maxOccurs="1"/>
			<xs:element name="timestamp" type="xocs:date-time-tp" nillable="false"/>
			<xs:element name="orig-load-date" type="xocs:date-tp" nillable="false"/>
			<xs:element name="datesort" type="xs:integer" nillable="false"/>
			<xs:element name="indexeddate" type="xocs:indexed-date-tp"/>
			<xs:element name="absavail" type="xs:integer"/>
			<xs:element name="suppressdummy" type="xocs:yes-no-string-tp"/>
			<xs:element name="srctype" type="xs:string"/>
			<xs:element name="subjareas" type="xocs:subj-areas-tp" minOccurs="0"/>
			<xs:element name="srctitle" type="xs:string"/>
			<xs:element name="countries" type="xocs:countries-tp" minOccurs="0"/>
			<xs:element name="languages" type="xocs:languages-tp" minOccurs="0"/>
			<xs:element name="prefnameauids" type="xocs:prefnameauids-tp" minOccurs="0"/>
			<xs:element name="open-access" type="xocs:open-access-type" minOccurs="0"/>
			<xs:element name="funding-list" type="xocs:funding-addon-type" minOccurs="0"/>
			<xs:element ref="cto:doctype" maxOccurs="unbounded"/>
			<xs:element ref="cto:group-id"/>
			<xs:element ref="cto:ref-id" minOccurs="0" maxOccurs="unbounded"/>
			<xs:element ref="cto:unique-author" minOccurs="0" maxOccurs="unbounded"/>
			<xs:element ref="cto:unique-auth-count"/>
		</xs:sequence>
	</xs:complexType>


	<!-- the following simple type states that the content must be a string with atleast one non Whitespace character -->
	<xs:simpleType name="nonEmptyString">
		<xs:restriction base="xs:string">
			<xs:minLength value="1"/>
			<xs:whiteSpace value="collapse"/>
		</xs:restriction>
	</xs:simpleType>

	<xs:simpleType name="online-status-tp">
		<xs:restriction base="xs:string">
			<xs:enumeration value="available"/>
			<xs:enumeration value="unavailable"/>
		</xs:restriction>
	</xs:simpleType>

	<!-- fulltext-eid score -->
	<xs:complexType name="fulltext-eid-tp">
		<xs:simpleContent>
			<xs:extension base="xs:string">
				<xs:attribute name="score" use="required" type="xs:string"/>
			</xs:extension>
		</xs:simpleContent>
	</xs:complexType>


	<!-- ========== Citation types ==========
        ab  Abstract Report
        ar  Article
        ba
        bk  Book
        br  Book Review
        bz  Business Article
        cb  Conference Abstract
        ch  Chapter
        cp  Conference Paper
        cr  Conference Review
        di  Dissertation
        ed  Editorial
        er  Erratum
        ip  Article in Press
        le  Letter
        no  Note
        pa  Patent
        pr  Press Release
        re  Review
        rf
        rp  Report
        sh  Short Survey
        tb  Tombstone
        wp  Working Paper
    -->
	<xs:simpleType name="citationtype.att">
		<xs:restriction base="xs:token">
			<xs:enumeration value="ab"/>
			<xs:enumeration value="ar"/>
			<xs:enumeration value="ba"/>
			<xs:enumeration value="bk"/>
			<xs:enumeration value="br"/>
			<xs:enumeration value="bz"/>
			<xs:enumeration value="cb"/>
			<xs:enumeration value="ch"/>
			<xs:enumeration value="cp"/>
			<xs:enumeration value="cr"/>
			<xs:enumeration value="di"/>
			<xs:enumeration value="ed"/>
			<xs:enumeration value="er"/>
			<xs:enumeration value="ip"/>
			<xs:enumeration value="le"/>
			<xs:enumeration value="no"/>
			<xs:enumeration value="pa"/>
			<xs:enumeration value="pr"/>
			<xs:enumeration value="re"/>
			<xs:enumeration value="rf"/>
			<xs:enumeration value="rp"/>
			<xs:enumeration value="sh"/>
			<xs:enumeration value="st"/>
			<xs:enumeration value="tb"/>
			<xs:enumeration value="wp"/>
		</xs:restriction>
	</xs:simpleType>


	<!-- ========== GenCitation types ==========
        bk  Book
        ch  Chapter
        co  Conference Proceeding
        cp  Conference Paper
        di  Dissertation
        ep  E-print
        ot  Other
        pa  Patent
        st  Standard
        tr  Technical Report
    -->
	<xs:simpleType name="gencitationtype.att">
		<xs:restriction base="xs:token">
			<xs:enumeration value="bk"/>
			<xs:enumeration value="ch"/>
			<xs:enumeration value="co"/>
			<xs:enumeration value="cp"/>
			<xs:enumeration value="di"/>
			<xs:enumeration value="ep"/>
			<xs:enumeration value="ot"/>
			<xs:enumeration value="pa"/>
			<xs:enumeration value="st"/>
			<xs:enumeration value="tr"/>
		</xs:restriction>
	</xs:simpleType>

	<!-- ========== Sequencebank types ==========
    -->
	<xs:simpleType name="sequencebanktype.att">
		<xs:restriction base="xs:token">
			<xs:enumeration value="DDBJ"/>
			<xs:enumeration value="EMBL"/>
			<xs:enumeration value="GENBANK"/>
			<xs:enumeration value="PIR"/>
			<xs:enumeration value="SWISSPROT"/>
			<xs:enumeration value="UNNAMED"/>
		</xs:restriction>
	</xs:simpleType>

	<!-- ========== Descriptor types ==========
    -->
	<xs:simpleType name="descriptortype.att">
		<xs:restriction base="xs:token">
			<xs:enumeration value="AUD"/>
			<xs:enumeration value="AUT"/>
			<xs:enumeration value="BLS"/>
			<xs:enumeration value="BTC"/>
			<xs:enumeration value="CBA"/>
			<xs:enumeration value="CBB"/>
			<xs:enumeration value="CBC"/>
			<xs:enumeration value="CBE"/>
			<xs:enumeration value="CCV"/>
			<xs:enumeration value="CFL"/>
			<xs:enumeration value="CLU"/>
			<xs:enumeration value="CMH"/>
			<xs:enumeration value="CNC"/>
			<xs:enumeration value="CSL"/>
			<xs:enumeration value="CSS"/>
			<xs:enumeration value="CSW"/>
			<xs:enumeration value="CTB"/>
			<xs:enumeration value="CTC"/>
			<xs:enumeration value="CTN"/>
			<xs:enumeration value="DID"/>
			<xs:enumeration value="DRA"/>
			<xs:enumeration value="DRG"/>
			<xs:enumeration value="ECN"/>
			<xs:enumeration value="FDE"/>
			<xs:enumeration value="GDE"/>
			<xs:enumeration value="GLY"/>
			<xs:enumeration value="KWD"/>
			<xs:enumeration value="MEA"/>
			<xs:enumeration value="MED"/>
			<xs:enumeration value="MGS"/>
			<xs:enumeration value="MKW"/>
			<xs:enumeration value="MLA"/>
			<xs:enumeration value="MMS"/>
			<xs:enumeration value="MSC"/>
			<xs:enumeration value="MSF"/>
			<xs:enumeration value="MSH"/>
			<xs:enumeration value="MUD"/>
			<xs:enumeration value="OZR"/>
			<xs:enumeration value="PCV"/>
			<xs:enumeration value="REA"/>
			<xs:enumeration value="RGI"/>
			<xs:enumeration value="SID"/>
			<xs:enumeration value="SPC"/>
			<xs:enumeration value="SPO"/>
			<xs:enumeration value="SUA"/>
			<xs:enumeration value="TRP"/>
			<xs:enumeration value="WDE"/>
			<xs:enumeration value="SCO"/>
			<xs:enumeration value="SCT"/>
			<xs:enumeration value="IBT"/>
			<xs:enumeration value="IGI"/>
			<xs:enumeration value="SCC"/>
			<xs:enumeration value="MDV"/>
		</xs:restriction>
	</xs:simpleType>

	<!-- ========== Classification types ==========
    -->
	<xs:simpleType name="classificationtype.att">
		<xs:restriction base="xs:token">
			<xs:enumeration value="ASJC"/>
			<xs:enumeration value="AUTARC-CONFIGURATION"/>
			<xs:enumeration value="CABSCLASS"/>
			<xs:enumeration value="CBNBGEO"/>
			<xs:enumeration value="CBNBSCOPE"/>
			<xs:enumeration value="CBNBSECTOR"/>
			<xs:enumeration value="CBNBSIC"/>
			<xs:enumeration value="CONFIGURATION-VALUE"/>
			<xs:enumeration value="CPXCLASS"/>
			<xs:enumeration value="EMCLASS"/>
			<xs:enumeration value="ESBD"/>
			<xs:enumeration value="COMPASSCLASS"/>
			<xs:enumeration value="ENCOMPASSCLASS"/>
			<xs:enumeration value="FLXCLASS"/>
			<xs:enumeration value="GEOCLASS"/>
			<xs:enumeration value="PATAUTO"/>
			<xs:enumeration value="PATENT-FAMILY"/>
			<xs:enumeration value="PATMANU"/>
			<xs:enumeration value="REAXYS-PRIORITY"/>
			<xs:enumeration value="SATURATION"/>
			<xs:enumeration value="SUBJECT"/>
			<xs:enumeration value="SUPPORT-INFORMATION"/>
			<xs:enumeration value="WTACLASS"/>
			<xs:enumeration value="SUBJABBR"/>
		</xs:restriction>
	</xs:simpleType>

	<!-- ========== Tradename types ==========
    -->
	<xs:simpleType name="tradenametype.att">
		<xs:restriction base="xs:token">
			<xs:enumeration value="TNM"/>
			<xs:enumeration value="TNV"/>
			<xs:enumeration value="TNW"/>
		</xs:restriction>
	</xs:simpleType>

	<!-- ========== Manufacturer types ==========
    -->
	<xs:simpleType name="manufacturertype.att">
		<xs:restriction base="xs:token">
			<xs:enumeration value="MNF"/>
			<xs:enumeration value="MNV"/>
			<xs:enumeration value="MNW"/>
		</xs:restriction>
	</xs:simpleType>

	<!-- ========== Chemical source types ==========
    -->
	<xs:simpleType name="chemicalsourcetype.att">
		<xs:restriction base="xs:token">
			<xs:enumeration value="esbd"/>
			<xs:enumeration value="nlm"/>
			<xs:enumeration value="cbnb"/>
		</xs:restriction>
	</xs:simpleType>

	<!-- ========== Source Types ==========
        b  Book
        d  Trade Journal
        j  Journal
        k  Book Series
        m  Multi-volume Reference Works
        p  Conference Proceeding
        r  Report
        n  Newsletter
        w  Newspaper
    -->
	<xs:simpleType name="sourcetype.att">
		<xs:restriction base="xs:token">
			<xs:enumeration value="b"/>
			<xs:enumeration value="d"/>
			<xs:enumeration value="j"/>
			<xs:enumeration value="k"/>
			<xs:enumeration value="m"/>
			<xs:enumeration value="p"/>
			<xs:enumeration value="r"/>
			<xs:enumeration value="n"/>
			<xs:enumeration value="w"/>
		</xs:restriction>
	</xs:simpleType>

	<!-- ========== Contributor Roles ==========
        auth  author
        comp  compiler
        edit  editor
        illu  illustrator
        phot  photographer
        publ  publisher
        revi  reviewer
        tran  translator
    -->
	<xs:simpleType name="contributorrole.att">
		<xs:restriction base="xs:token">
			<xs:enumeration value="auth"/>
			<xs:enumeration value="comp"/>
			<xs:enumeration value="edit"/>
			<xs:enumeration value="illu"/>
			<xs:enumeration value="phot"/>
			<xs:enumeration value="publ"/>
			<xs:enumeration value="revi"/>
			<xs:enumeration value="tran"/>
		</xs:restriction>
	</xs:simpleType>

	<xs:simpleType name="yesno.att">
		<xs:restriction base="xs:token">
			<xs:enumeration value="y"/>
			<xs:enumeration value="n"/>
		</xs:restriction>
	</xs:simpleType>

	<xs:group name="pubdateType">
		<xs:sequence>
			<xs:element name="year" type="xs:string" minOccurs="0" form="unqualified"/>
			<xs:element name="month" type="xs:string" minOccurs="0" form="unqualified"/>
			<xs:element name="day" type="xs:string" minOccurs="0" form="unqualified"/>
			<xs:element name="date-text" type="xocs:date-text-tp" minOccurs="0" form="unqualified"/>
		</xs:sequence>
	</xs:group>
	<!--xs:group name="pubdateType">
		<xs:sequence>
			<xs:sequence minOccurs="0" maxOccurs="1">
				<xs:element name="year" type="xs:string" form="unqualified"/>
				<xs:choice minOccurs="0">
					<xs:sequence>
						<xs:element name="month" type="xs:string" form="unqualified"/>
						<xs:element name="day" type="xs:string" minOccurs="0" form="unqualified"/>
					</xs:sequence>
				</xs:choice>
			</xs:sequence>
			<xs:element name="date-text" type="xocs:date-text-tp" form="unqualified"/>
		</xs:sequence>
	</xs:group-->

	<xs:complexType name="date-text-tp" mixed="true">
		<xs:simpleContent>
			<xs:extension base="xs:string">
				<xs:attribute name="xfab-added" use="optional" type="xs:boolean"/>
			</xs:extension>
		</xs:simpleContent>
	</xs:complexType>

	<xs:complexType name="personalnameType">
		<xs:sequence>
			<xs:element minOccurs="0" ref="ce:initials"/>
			<xs:element minOccurs="0" ref="ce:indexed-name"/>
			<xs:choice minOccurs="0">
				<xs:sequence>
					<xs:element minOccurs="0" ref="ce:degrees"/>
					<xs:element ref="ce:surname"/>
					<xs:element minOccurs="0" ref="ce:given-name"/>
					<xs:element minOccurs="0" ref="ce:suffix"/>
					<xs:element minOccurs="0" ref="ce:alt-name"/>
				</xs:sequence>
				<xs:element name="nametext" type="xs:string" form="unqualified"/>
			</xs:choice>
		</xs:sequence>
		<xs:attribute name="author-instance-id" type="xs:NMTOKEN"/>
	</xs:complexType>

	<xs:complexType name="affiliationIdType">
		<xs:attribute name="afid" type="xs:positiveInteger"/>
		<xs:attribute name="dptid" type="xs:positiveInteger" use="optional"/>
	</xs:complexType>


	<xs:complexType name="affiliationType">
		<xs:sequence>
			<xs:choice>
				<xs:element ref="ce:text"/>
				<xs:sequence>
					<xs:element name="organization" type="xocs:organization-tp" minOccurs="0" maxOccurs="unbounded" form="unqualified"/>
					<xs:element name="address-part" type="xocs:address-part-tp" minOccurs="0" form="unqualified"/>
					<xs:choice minOccurs="0">
						<xs:element name="city-group" type="xs:string" form="unqualified"/>
						<xs:sequence>
							<xs:element name="city" type="xs:string" minOccurs="0" form="unqualified"/>
							<xs:element name="state" type="xs:string" minOccurs="0" form="unqualified"/>
							<xs:element name="postal-code" type="xocs:postal-code-tp" minOccurs="0" maxOccurs="unbounded" form="unqualified"/>
						</xs:sequence>
					</xs:choice>
				</xs:sequence>
			</xs:choice>
			<xs:element name="affiliation-id" minOccurs="0" maxOccurs="unbounded" type="xocs:affiliationIdType" form="unqualified"/>
			<xs:element name="country" type="xs:string" minOccurs="0" maxOccurs="1" form="unqualified"/>
		</xs:sequence>
		<xs:attribute name="affiliation-instance-id" type="xs:NMTOKEN"/>
	</xs:complexType>

	<xs:complexType name="classification-tp" mixed="true">
		<xs:choice minOccurs="0" maxOccurs="unbounded">
			<xs:element name="classification-code" type="xs:string" form="unqualified"/>
			<xs:element name="classification-description" type="xs:string" form="unqualified"/>
		</xs:choice>
		<xs:attribute name="weight">
			<xs:simpleType>
				<xs:restriction base="xs:token">
					<xs:enumeration value="-"/>
					<xs:enumeration value="?"/>
					<xs:enumeration value="+"/>
					<xs:enumeration value="++"/>
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
	</xs:complexType>

	<!-- ========== Wrapper and process info ========== -->

	<xs:complexType name="item-tp">
		<xs:sequence>
			<xs:element name="item" type="xocs:origItem-tp" form="unqualified"/>
		</xs:sequence>
	</xs:complexType>

	<xs:complexType name="origItem-tp">
		<xs:sequence>
			<xs:element ref="ait:process-info"/>
			<xs:element name="bibrecord" type="xocs:bibrecord-tp" form="unqualified"/>
		</xs:sequence>
	</xs:complexType>

	<xs:complexType name="bibrecord-tp">
		<xs:sequence>
			<xs:element name="item-info" type="xocs:item-info-tp" form="unqualified"/>
			<xs:element name="itemfilelinks" type="xocs:itemfilelinks-tp" minOccurs="0" form="unqualified"/>
			<xs:element name="head" type="xocs:head-tp" form="unqualified"/>
			<xs:element name="tail" type="xocs:tail-tp" minOccurs="0" form="unqualified"/>
		</xs:sequence>
	</xs:complexType>

	<!-- ========== Bibliographic record ========== -->

	<xs:complexType name="item-info-tp">
		<xs:sequence>
			<xs:element name="copyright" type="xocs:copyright-tp" maxOccurs="unbounded" form="unqualified"/>
			<xs:element name="itemidlist" type="xocs:itemidlist-tp" form="unqualified"/>
			<xs:element name="history" type="xocs:history-tp" form="unqualified"/>
			<xs:element name="dbcollection" type="xs:string" minOccurs="0" maxOccurs="unbounded" form="unqualified"/>
			<xs:element name="external-source" type="xs:string" minOccurs="0" form="unqualified"/>
		</xs:sequence>
	</xs:complexType>

	<xs:complexType name="itemfilelinks-tp">
		<xs:sequence>
			<xs:element name="itemfilelink" type="xocs:itemfilelink-tp" maxOccurs="unbounded" form="unqualified"/>
		</xs:sequence>
	</xs:complexType>

	<xs:complexType name="itemfilelink-tp">
		<xs:attribute name="href" use="required"/>
		<xs:attribute name="MIME-type"/>
	</xs:complexType>

	<xs:complexType name="copyright-tp" mixed="true">
		<xs:attribute name="type"/>
	</xs:complexType>

	<xs:complexType name="itemidlist-tp">
		<xs:sequence>
			<xs:element ref="ce:pii" minOccurs="0" maxOccurs="1"/>
			<xs:element ref="ce:doi" minOccurs="0" maxOccurs="1"/>
			<xs:element name="itemid" type="xocs:itemid-tp" maxOccurs="unbounded" form="unqualified"/>
		</xs:sequence>
	</xs:complexType>

	<xs:complexType name="itemid-tp" mixed="true">
		<xs:simpleContent>
			<xs:extension base="xs:string">
				<xs:attribute name="idtype" type="xs:NMTOKEN" use="required"/>
			</xs:extension>
		</xs:simpleContent>
	</xs:complexType>

	<!-- Item history elements -->

	<xs:complexType name="history-tp">
		<xs:sequence>
			<xs:element name="date-created" type="xocs:date-created-tp" form="unqualified"/>
			<xs:element name="date-completed" type="xocs:date-completed-tp" minOccurs="0" form="unqualified"/>
			<xs:element name="date-revised" type="xocs:date-revised-tp" minOccurs="0" maxOccurs="unbounded" form="unqualified"/>
		</xs:sequence>
	</xs:complexType>

	<xs:complexType name="date-created-tp">
		<xs:attributeGroup ref="ce:date.att"/>
	</xs:complexType>
	<xs:complexType name="date-completed-tp">
		<xs:attributeGroup ref="ce:date.att"/>
	</xs:complexType>

	<xs:complexType name="date-revised-tp">
		<xs:simpleContent>
			<xs:extension base="xocs:date-content-tp">
				<xs:attribute name="year" type="xs:NMTOKEN" use="required"/>
				<xs:attribute name="month" type="xs:NMTOKEN" use="required"/>
				<xs:attribute name="day" type="xs:NMTOKEN" use="required"/>
				<xs:attribute name="timestamp" type="xs:dateTime" use="optional"/>
			</xs:extension>
		</xs:simpleContent>
	</xs:complexType>

	<xs:simpleType name="date-content-tp">
		<xs:restriction base="xs:string">
			<xs:pattern value="(.{0})|([0-9]{8})"/>
		</xs:restriction>
	</xs:simpleType>

	<!-- ========== head ========== -->

	<xs:complexType name="head-tp">
		<xs:sequence>
			<xs:element name="citation-info" type="xocs:citation-info-tp" minOccurs="0" form="unqualified"/>
			<xs:element name="related-item" type="xocs:related-item-tp" minOccurs="0" maxOccurs="unbounded" form="unqualified"/>
			<xs:element name="citation-title" type="xocs:citation-title-tp" minOccurs="0" form="unqualified"/>
			<xs:element name="author-group" type="xocs:author-group-tp" minOccurs="0" maxOccurs="unbounded" form="unqualified"/>
			<xs:element name="correspondence" type="xocs:correspondence-tp" minOccurs="0" form="unqualified"/>
			<xs:element name="grantlist" type="xocs:grantlist-tp" minOccurs="0" form="unqualified"/>
			<xs:element name="abstracts" type="xocs:abstracts-tp" minOccurs="0" form="unqualified"/>
			<xs:element name="source" type="xocs:source-tp" minOccurs="0" form="unqualified"/>
			<xs:element name="enhancement" type="xocs:enhancement-tp" minOccurs="0" form="unqualified"/>
		</xs:sequence>
	</xs:complexType>

	<!-- Citation elements -->
	<xs:complexType name="citation-info-tp">
		<xs:sequence>
			<xs:element name="citation-type" type="xocs:citation-type-tp" minOccurs="0" maxOccurs="unbounded" form="unqualified"/>
			<xs:element name="citation-language" type="xocs:citation-language-tp" minOccurs="0" maxOccurs="unbounded" form="unqualified"/>
			<xs:element name="abstract-language" type="xocs:abstract-language-tp" minOccurs="0" maxOccurs="unbounded" form="unqualified"/>
			<xs:element name="patent" type="xocs:patent-tp" minOccurs="0" form="unqualified"/>
			<xs:element name="author-keywords" type="xocs:author-keywords-tp" minOccurs="0" form="unqualified"/>
			<xs:element name="dummy-link" type="xocs:dummy-link-tp" minOccurs="0" form="unqualified"/>
			<xs:element name="figure-information" type="xs:string" minOccurs="0" form="unqualified"/>
		</xs:sequence>
	</xs:complexType>

	<xs:complexType name="citation-type-tp">
		<xs:attribute name="code" use="required" type="xocs:citationtype.att"/>
	</xs:complexType>
	<xs:complexType name="citation-language-tp">
		<xs:attribute ref="xml:lang" use="required"/>
		<xs:attribute name="language" use="required" type="xs:string"/>
	</xs:complexType>
	<xs:complexType name="abstract-language-tp">
		<xs:attribute ref="xml:lang" use="required"/>
		<xs:attribute name="language" use="required" type="xs:string"/>
	</xs:complexType>
	<xs:complexType name="author-keywords-tp">
		<xs:sequence>
			<xs:element name="author-keyword" type="xocs:author-keyword-tp" maxOccurs="unbounded" form="unqualified"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="author-keyword-tp" mixed="true">
		<xs:group minOccurs="0" maxOccurs="unbounded" ref="ani:richstring.data"/>
		<xs:attribute name="perspective"/>
		<xs:attribute ref="xml:lang" default="eng"/>
		<xs:attribute name="original" type="xocs:yesno.att"/>
	</xs:complexType>
	<xs:complexType name="dummy-link-tp">
		<xs:sequence>
			<xs:element name="gen-citationtype" type="xocs:gen-citationtype-tp" form="unqualified"/>
			<xs:element name="itemlink" type="xs:string" form="unqualified"/>
		</xs:sequence>
		<xs:attribute name="restricted-access" type="xocs:yesno.att" use="required"/>
	</xs:complexType>
	<xs:complexType name="gen-citationtype-tp">
		<xs:attribute name="code" use="required" type="xocs:gencitationtype.att"/>
	</xs:complexType>

	<xs:complexType name="related-item-tp">
		<xs:sequence>
			<xs:element minOccurs="0" ref="ce:pii"/>
			<xs:element minOccurs="0" ref="ce:doi"/>
			<xs:element name="itemid" type="xocs:itemid-tp" minOccurs="0" maxOccurs="unbounded" form="unqualified"/>
			<xs:element name="citation-info" type="xocs:citation-info-tp" minOccurs="0" form="unqualified"/>
			<xs:element name="citation-title" type="xocs:citation-title-tp" minOccurs="0" form="unqualified"/>
			<xs:element name="contributor-group" type="xocs:contributor-group-tp" minOccurs="0" maxOccurs="unbounded" form="unqualified"/>
			<xs:element name="source" type="xocs:source-tp" minOccurs="0" form="unqualified"/>
		</xs:sequence>
		<xs:attribute name="type" use="required"/>
	</xs:complexType>
	<xs:complexType name="citation-title-tp">
		<xs:sequence>
			<xs:element name="titletext" type="xocs:titletext-tp" maxOccurs="unbounded" form="unqualified"/>
		</xs:sequence>
	</xs:complexType>

	<xs:complexType name="titletext-tp" mixed="true">
		<xs:group minOccurs="0" maxOccurs="unbounded" ref="ani:richstring.data"/>
		<xs:attribute name="perspective"/>
		<xs:attribute ref="xml:lang" default="eng"/>
		<xs:attribute name="language" type="xs:string"/>
		<xs:attribute name="original" type="xocs:yesno.att"/>
	</xs:complexType>
	<xs:complexType name="contributor-group-tp">
		<xs:sequence>
			<xs:choice minOccurs="0" maxOccurs="unbounded">
				<xs:element name="contributor" type="xocs:contributor-tp" form="unqualified"/>
				<xs:element name="collaboration" type="xocs:collaboration-tp" form="unqualified"/>
			</xs:choice>
			<xs:element name="et-al" type="xocs:et-al-tp" minOccurs="0" form="unqualified"/>
			<xs:element name="affiliation" type="xocs:affiliation-tp" minOccurs="0" form="unqualified"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="contributor-tp">
		<xs:complexContent>
			<xs:extension base="xocs:personalnameType">
				<xs:sequence>
					<xs:element minOccurs="0" ref="ce:e-address"/>
				</xs:sequence>
				<xs:attribute name="role" use="required" type="xocs:contributorrole.att"/>
				<xs:attribute name="auid"/>
				<xs:attribute name="seq" use="required"/>
				<xs:attribute name="type"/>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>

	<!-- AuthorGroup elements -->

	<xs:complexType name="author-group-tp">
		<xs:sequence>
			<xs:choice minOccurs="0" maxOccurs="unbounded">
				<xs:element name="author" type="xocs:author-tp" form="unqualified"/>
				<xs:element name="collaboration" type="xocs:collaboration-tp" form="unqualified"/>
			</xs:choice>
			<xs:element name="et-al" type="xocs:et-al-tp" minOccurs="0" form="unqualified"/>
			<xs:element name="affiliation" type="xocs:affiliation-tp" minOccurs="0" form="unqualified"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="author-tp">
		<xs:complexContent>
			<xs:extension base="xocs:personalnameType">
				<xs:sequence>
					<xs:element name="preferred-name" type="xocs:personalnameType" minOccurs="0" form="unqualified"/>
					<xs:element minOccurs="0" ref="ce:e-address"/>
				</xs:sequence>
				<xs:attribute name="auid"/>
				<xs:attribute name="seq" use="required"/>
				<xs:attribute name="type"/>
				<xs:attribute name="orcid"/>
				<xs:attribute name="date-locked" type="xs:dateTime"/>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="collaboration-tp">
		<xs:sequence>
			<xs:element ref="ce:indexed-name"/>
			<xs:element ref="ce:text"/>
		</xs:sequence>
		<xs:attribute name="seq" use="required"/>
		<xs:attribute name="collaboration-instance-id" type="xs:NMTOKEN"/>
	</xs:complexType>
	<xs:complexType name="et-al-tp"/>
	<xs:complexType name="affiliation-tp">
		<xs:complexContent>
			<xs:extension base="xocs:affiliationType">
				<xs:attribute name="afid"/>
				<xs:attribute name="dptid"/>
				<xs:attribute name="country"/>
				<xs:attribute name="date-locked" type="xs:dateTime"/>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="organization-tp" mixed="true">
		<xs:group minOccurs="0" maxOccurs="unbounded" ref="ani:richstring.data"/>
	</xs:complexType>
	<xs:complexType name="address-part-tp" mixed="true">
		<xs:group minOccurs="0" maxOccurs="unbounded" ref="ani:richstring.data"/>
	</xs:complexType>
	<xs:complexType name="postal-code-tp" mixed="true">
		<xs:attribute name="type">
			<xs:simpleType>
				<xs:restriction base="xs:token">
					<xs:enumeration value="pre"/>
					<xs:enumeration value="post"/>
					<xs:enumeration value="zip"/>
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
	</xs:complexType>
	<xs:complexType name="country-tp">
		<xs:attribute name="iso-code" type="xocs:iso-code-tp" use="required"/>
	</xs:complexType>

	<!-- Correspondence Address elements -->

	<xs:complexType name="correspondence-tp">
		<xs:sequence>
			<xs:element name="person" type="xocs:personalnameType" minOccurs="0" form="unqualified"/>
			<xs:element name="affiliation" type="xocs:affiliation-tp" minOccurs="0" form="unqualified"/>
			<xs:element minOccurs="0" ref="ce:e-address"/>
		</xs:sequence>
	</xs:complexType>

	<!-- Grant elements -->

	<xs:complexType name="grantlist-tp">
		<xs:sequence>
			<xs:element name="grant" type="xocs:grant-tp" minOccurs="0" maxOccurs="unbounded" form="unqualified"/>
			<xs:element name="grant-text" type="xocs:grant-text-tp" minOccurs="0" form="unqualified"/>
		</xs:sequence>
		<xs:attribute name="complete" default="y" type="xocs:yesno.att"/>
	</xs:complexType>
	<xs:complexType name="grant-tp">
		<xs:sequence>
			<xs:element name="grant-id" type="xs:string" minOccurs="0" form="unqualified"/>
			<xs:element name="grant-acronym" type="xs:string" minOccurs="0" form="unqualified"/>
			<xs:element name="grant-agency" type="xocs:grand-agency-tp" form="unqualified"/>
			<xs:element name="grant-agency-id" type="xs:string" minOccurs="0" form="unqualified"/>
		</xs:sequence>
	</xs:complexType>

	<!-- grand-agency -->
	<xs:complexType name="grand-agency-tp" mixed="true">
		<xs:attribute name="iso-code" use="optional" type="xocs:iso-code-tp"/>
	</xs:complexType>

	<xs:complexType name="grant-text-tp" mixed="true">
		<xs:group minOccurs="0" maxOccurs="unbounded" ref="ani:richstring.data"/>
		<xs:attribute ref="xml:lang" default="eng"/>
	</xs:complexType>

	<!-- Source elements -->

	<xs:complexType name="source-tp">
		<xs:sequence>
			<xs:element name="sourcetitle" type="xocs:sourcetitle-tp" minOccurs="0" form="unqualified"/>
			<xs:element name="preferred-sourcetitle" type="xocs:preferred-sourcetitle-tp" minOccurs="0" form="unqualified"/>
			<xs:element name="sourcetitle-abbrev" type="xocs:sourcetitle-abbrev-tp" minOccurs="0" form="unqualified"/>
			<xs:element name="translated-sourcetitle" type="xocs:translated-sourcetitle-tp" minOccurs="0" maxOccurs="unbounded" form="unqualified"/>
			<xs:element name="volumetitle" type="xocs:volumetitle-tp" minOccurs="0" form="unqualified"/>
			<xs:element name="issuetitle" type="xocs:issuetitle-tp" minOccurs="0" form="unqualified"/>
			<xs:element name="issue-designation" type="xocs:issue-designation-tp" minOccurs="0" form="unqualified"/>
			<xs:element name="issn" type="xocs:issn-tp" minOccurs="0" maxOccurs="unbounded" form="unqualified"/>
			<xs:element name="isbn" type="xocs:isbn-tp" minOccurs="0" maxOccurs="unbounded" form="unqualified"/>
			<xs:element name="codencode" type="xs:string" minOccurs="0" form="unqualified"/>
			<xs:element name="part" type="xs:string" minOccurs="0" form="unqualified"/>
			<xs:element name="volisspag" type="xocs:volisspag-tp" minOccurs="0" form="unqualified"/>
			<xs:element name="article-number" type="xs:string" minOccurs="0" form="unqualified"/>
			<xs:element name="publicationyear" type="xocs:publicationyear-tp" minOccurs="0" form="unqualified"/>
			<xs:element name="publicationdate" type="xocs:publicationdate-tp" minOccurs="0" form="unqualified"/>
			<xs:element name="website" type="xocs:website-tp" minOccurs="0" maxOccurs="unbounded" form="unqualified"/>
			<xs:element name="contributor-group" type="xocs:contributor-group-tp" minOccurs="0" maxOccurs="unbounded" form="unqualified"/>
			<xs:element name="editors" type="xocs:editors-tp" minOccurs="0" form="unqualified"/>
			<xs:element name="publisher" type="xocs:publisher-tp" minOccurs="0" maxOccurs="unbounded" form="unqualified"/>
			<xs:element name="additional-srcinfo" type="xocs:additional-srcinfo-tp" minOccurs="0" form="unqualified"/>
			<xs:element name="bib-text" type="xs:string" minOccurs="0" form="unqualified"/>
		</xs:sequence>
		<xs:attribute name="srcid"/>
		<xs:attribute name="type" type="xocs:sourcetype.att"/>
		<xs:attribute name="country"/>
	</xs:complexType>

	<xs:complexType name="sourcetitle-tp" mixed="true">
		<xs:group minOccurs="0" maxOccurs="unbounded" ref="ani:richstring.data"/>
		<xs:attribute name="xfab-added" use="optional" type="xs:boolean"/>
	</xs:complexType>
	<xs:complexType name="preferred-sourcetitle-tp" mixed="true">
		<xs:group minOccurs="0" maxOccurs="unbounded" ref="ani:richstring.data"/>
	</xs:complexType>
	<xs:complexType name="sourcetitle-abbrev-tp" mixed="true">
		<xs:group minOccurs="0" maxOccurs="unbounded" ref="ani:richstring.data"/>
	</xs:complexType>
	<xs:complexType name="translated-sourcetitle-tp" mixed="true">
		<xs:group minOccurs="0" maxOccurs="unbounded" ref="ani:richstring.data"/>
		<xs:attribute ref="xml:lang"/>
	</xs:complexType>
	<xs:complexType name="volumetitle-tp" mixed="true">
		<xs:group minOccurs="0" maxOccurs="unbounded" ref="ani:richstring.data"/>
	</xs:complexType>
	<xs:complexType name="issuetitle-tp" mixed="true">
		<xs:group minOccurs="0" maxOccurs="unbounded" ref="ani:richstring.data"/>
	</xs:complexType>
	<xs:complexType name="issue-designation-tp" mixed="true">
		<xs:group minOccurs="0" maxOccurs="unbounded" ref="ani:richstring.data"/>
	</xs:complexType>

	<xs:complexType name="issn-tp" mixed="true">
		<xs:attribute name="type">
			<xs:simpleType>
				<xs:restriction base="xs:token">
					<xs:enumeration value="print"/>
					<xs:enumeration value="electronic"/>
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
	</xs:complexType>
	<xs:complexType name="isbn-tp" mixed="true">
		<xs:attribute name="type"/>
		<xs:attribute name="length">
			<xs:simpleType>
				<xs:restriction base="xs:token">
					<xs:enumeration value="10"/>
					<xs:enumeration value="13"/>
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
		<xs:attribute name="level">
			<xs:simpleType>
				<xs:restriction base="xs:token">
					<xs:enumeration value="set"/>
					<xs:enumeration value="volume"/>
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
	</xs:complexType>

	<xs:complexType name="volisspag-tp">
		<xs:choice>
			<xs:sequence>
				<xs:element name="voliss" type="xocs:voliss-tp" form="unqualified"/>
				<xs:element name="supplement" type="xs:string" minOccurs="0" form="unqualified"/>
				<xs:choice minOccurs="0">
					<xs:element name="pagerange" type="xocs:pagerange-tp" form="unqualified"/>
					<xs:element name="pages" type="xs:string" form="unqualified"/>
				</xs:choice>
				<xs:element name="pagecount" type="xocs:pagecount-tp" minOccurs="0" maxOccurs="unbounded" form="unqualified"/>
			</xs:sequence>
			<xs:sequence>
				<xs:choice>
					<xs:element name="pagerange" type="xocs:pagerange-tp" form="unqualified"/>
					<xs:element name="pages" type="xs:string" form="unqualified"/>
					<xs:element name="pagecount" type="xocs:pagecount-tp" form="unqualified"/>
				</xs:choice>
				<xs:element name="pagecount" type="xocs:pagecount-tp" minOccurs="0" maxOccurs="unbounded" form="unqualified"/>
			</xs:sequence>
		</xs:choice>
	</xs:complexType>
	<xs:complexType name="voliss-tp">
		<xs:attribute name="volume"/>
		<xs:attribute name="issue"/>
	</xs:complexType>

	<xs:complexType name="pagerange-tp">
		<xs:attribute name="first" use="required"/>
		<xs:attribute name="last"/>
	</xs:complexType>
	<xs:complexType name="pagecount-tp" mixed="true">
		<xs:attribute name="type" default="arabic">
			<xs:simpleType>
				<xs:restriction base="xs:token">
					<xs:enumeration value="arabic"/>
					<xs:enumeration value="roman"/>
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
	</xs:complexType>

	<xs:complexType name="publicationyear-tp">
		<xs:attribute name="first" use="required"/>
		<xs:attribute name="last"/>
	</xs:complexType>
	<xs:complexType name="publicationdate-tp">
		<xs:sequence>
			<xs:group ref="xocs:pubdateType"/>
		</xs:sequence>
	</xs:complexType>

	<xs:complexType name="website-tp">
		<xs:choice>
			<xs:sequence>
				<xs:element name="websitename" type="xs:string" form="unqualified"/>
				<xs:element minOccurs="0" ref="ce:e-address"/>
			</xs:sequence>
			<xs:element ref="ce:e-address"/>
		</xs:choice>
		<xs:attribute name="type">
			<xs:simpleType>
				<xs:restriction base="xs:token">
					<xs:enumeration value="item"/>
					<xs:enumeration value="source"/>
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
	</xs:complexType>

	<xs:complexType name="editors-tp">
		<xs:sequence>
			<xs:element name="editor" type="xocs:editor-tp" maxOccurs="unbounded" form="unqualified"/>
		</xs:sequence>
		<xs:attribute name="complete" type="xocs:yesno.att" default="y"/>
	</xs:complexType>
	<xs:complexType name="editor-tp">
		<xs:complexContent>
			<xs:extension base="xocs:personalnameType">
				<xs:attribute name="role"/>
				<xs:attribute name="type"/>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>

	<xs:complexType name="publisher-tp">
		<xs:sequence>
			<xs:element name="publishername" type="xs:string" minOccurs="0" form="unqualified"/>
			<xs:choice minOccurs="0">
				<xs:element name="publisheraddress" type="xs:string" form="unqualified"/>
				<xs:element name="affiliation" type="xocs:affiliation-tp" form="unqualified"/>
			</xs:choice>
			<xs:element minOccurs="0" ref="ce:e-address"/>
		</xs:sequence>
	</xs:complexType>

	<xs:complexType name="additional-srcinfo-tp">
		<xs:sequence>
			<xs:element name="secondaryjournal" type="xocs:secondaryjournal-tp" minOccurs="0" form="unqualified"/>
			<xs:element name="conferenceinfo" type="xocs:conferenceinfo-tp" minOccurs="0" form="unqualified"/>
			<xs:element name="reportinfo" type="xocs:reportinfo-tp" minOccurs="0" form="unqualified"/>
		</xs:sequence>
	</xs:complexType>

	<xs:complexType name="secondaryjournal-tp">
		<xs:sequence>
			<xs:element name="sourcetitle" type="xocs:sourcetitle-tp" minOccurs="0" form="unqualified"/>
			<xs:element name="sourcetitle-abbrev" type="xocs:sourcetitle-abbrev-tp" minOccurs="0" form="unqualified"/>
			<xs:element name="issn" type="xocs:issn-tp" minOccurs="0" form="unqualified"/>
			<xs:element name="publicationyear" type="xocs:publicationyear-tp" minOccurs="0" form="unqualified"/>
			<xs:element name="publicationdate" type="xocs:publicationdate-tp" minOccurs="0" form="unqualified"/>
			<xs:element name="voliss" type="xocs:voliss-tp" minOccurs="0" form="unqualified"/>
		</xs:sequence>
	</xs:complexType>

	<xs:complexType name="conferenceinfo-tp">
		<xs:sequence>
			<xs:element name="confevent" type="xocs:confevent-tp" minOccurs="0" form="unqualified"/>
			<xs:element name="confpublication" type="xocs:confpublication-tp" minOccurs="0" form="unqualified"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="confevent-tp">
		<xs:sequence>
			<xs:element name="confname" type="xocs:confname-tp" minOccurs="0" form="unqualified"/>
			<xs:element name="confnumber" type="xocs:confnumber-tp" minOccurs="0" form="unqualified"/>
