			<xs:element name="tradename" type="xs:string" form="unqualified"/>
			<xs:element name="manufacturer" type="xocs:manufacturer-tp" minOccurs="0" form="unqualified"/>
		</xs:sequence>
	</xs:complexType>

	<!-- SequenceBankList elements -->

	<xs:complexType name="sequencebanks-tp">
		<xs:sequence>
			<xs:element name="sequencebank" type="xocs:sequencebank-tp" maxOccurs="unbounded" form="unqualified"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="sequencebank-tp">
		<xs:sequence>
			<xs:element name="sequence-number" type="xocs:sequence-number-tp" maxOccurs="unbounded" form="unqualified"/>
		</xs:sequence>
		<xs:attribute name="name" use="required" type="xocs:sequencebanktype.att"/>
		<xs:attribute name="complete" type="xocs:yesno.att" default="y"/>
	</xs:complexType>
	<xs:complexType name="sequence-number-tp" mixed="true">
		<xs:attributeGroup ref="xocs:sequence-number.attlist"/>
	</xs:complexType>
	<xs:attributeGroup name="sequence-number.attlist">
		<xs:attribute name="type" default="referenced">
			<xs:simpleType>
				<xs:restriction base="xs:token">
					<xs:enumeration value="submitted"/>
					<xs:enumeration value="referenced"/>
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
	</xs:attributeGroup>

	<!-- Chemical group elements -->

	<xs:complexType name="chemicalgroup-tp">
		<xs:sequence>
			<xs:element name="chemicals" type="xocs:chemicals-tp" maxOccurs="unbounded" form="unqualified"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="chemicals-tp">
		<xs:sequence>
			<xs:element name="chemical" type="xocs:chemical-tp" maxOccurs="unbounded" form="unqualified"/>
		</xs:sequence>
		<xs:attribute name="source" type="xocs:chemicalsourcetype.att" default="esbd"/>
	</xs:complexType>
	<xs:complexType name="chemical-tp">
		<xs:sequence>
			<xs:element name="chemical-name" type="xs:string" form="unqualified"/>
			<xs:element name="cas-registry-number" type="xs:string" minOccurs="0" maxOccurs="unbounded" form="unqualified"/>
			<xs:element name="enzyme-commission-number" type="xs:string" minOccurs="0" maxOccurs="unbounded" form="unqualified"/>
		</xs:sequence>
	</xs:complexType>

	<!-- ========== tail ========== -->

	<xs:complexType name="tail-tp">
		<xs:sequence>
			<xs:element name="bibliography" type="xocs:bibliography-tp" form="unqualified"/>
		</xs:sequence>
	</xs:complexType>

	<!-- ReferenceList elements -->

	<xs:complexType name="bibliography-tp">
		<xs:sequence>
			<xs:element name="reference" type="xocs:reference-tp" minOccurs="0" maxOccurs="unbounded" form="unqualified"/>
		</xs:sequence>
		<xs:attribute name="refcount" use="required"/>
	</xs:complexType>

	<xs:complexType name="reference-tp">
		<xs:sequence>
			<xs:element name="ref-info" type="xocs:ref-info-tp" form="unqualified"/>
			<xs:element name="ref-fulltext" type="xocs:ref-fulltext-tp" minOccurs="0" form="unqualified"/>
			<!--xs:element name="refd-itemcitation" type="xocs:refd-itemcitation-tp" minOccurs="0" form="unqualified"/ -->
		</xs:sequence>
		<xs:attribute name="id" type="xs:NMTOKEN"/>
		<xs:attribute name="date-locked" type="xs:dateTime"/>
	</xs:complexType>

	<xs:complexType name="ref-info-tp">
		<xs:sequence>
			<xs:element name="ref-title" type="xocs:ref-title-tp" minOccurs="0" form="unqualified"/>
			<xs:element name="refd-itemidlist" type="xocs:refd-itemidlist-tp" minOccurs="0" form="unqualified"/>
			<xs:element name="ref-authors" type="xocs:ref-authors-tp" minOccurs="0" form="unqualified"/>
			<xs:element name="ref-sourcetitle" type="xocs:ref-sourcetitle-tp" minOccurs="0" form="unqualified"/>
			<xs:element name="ref-publicationyear" type="xocs:ref-publicationyear-tp" minOccurs="0" form="unqualified"/>
			<xs:element name="ref-volisspag" type="xocs:ref-volisspag-tp" minOccurs="0" form="unqualified"/>
			<xs:element name="ref-website" type="xocs:ref-website-tp" minOccurs="0" form="unqualified"/>
			<xs:element name="ref-text" type="xocs:ref-text-tp" minOccurs="0" form="unqualified"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="ref-title-tp">
		<xs:sequence>
			<xs:element name="ref-titletext" type="xocs:ref-titletext-tp" maxOccurs="unbounded" form="unqualified"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="ref-titletext-tp" mixed="true">
		<xs:group minOccurs="0" maxOccurs="unbounded" ref="ani:richstring.data"/>
	</xs:complexType>
	<xs:complexType name="refd-itemidlist-tp">
		<xs:sequence>
			<xs:element name="itemid" type="xocs:itemid-tp" maxOccurs="unbounded" form="unqualified"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="ref-authors-tp">
		<xs:sequence>
			<xs:choice minOccurs="0" maxOccurs="unbounded">
				<xs:element name="author" type="xocs:author-tp" form="unqualified"/>
				<xs:element name="collaboration" type="xocs:collaboration-tp" form="unqualified"/>
			</xs:choice>
			<xs:element name="et-al" type="xocs:et-al-tp" minOccurs="0" form="unqualified"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="ref-sourcetitle-tp" mixed="true">
		<xs:group minOccurs="0" maxOccurs="unbounded" ref="ani:richstring.data"/>
	</xs:complexType>
	<xs:complexType name="ref-publicationyear-tp">
		<xs:attribute name="first" use="required"/>
		<xs:attribute name="last"/>
	</xs:complexType>
	<xs:complexType name="ref-volisspag-tp">
		<xs:sequence>
			<xs:element name="voliss" type="xocs:voliss-tp" minOccurs="0" form="unqualified"/>
			<xs:choice minOccurs="0">
				<xs:element name="pagerange" type="xocs:pagerange-tp" form="unqualified"/>
				<xs:element name="pages" type="xs:string" form="unqualified"/>
				<xs:element name="pagecount" type="xocs:pagecount-tp" form="unqualified"/>
			</xs:choice>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="ref-website-tp">
		<xs:choice>
			<xs:sequence>
				<xs:element name="websitename" type="xs:string" form="unqualified"/>
				<xs:element minOccurs="0" ref="ce:e-address"/>
			</xs:sequence>
			<xs:element ref="ce:e-address"/>
		</xs:choice>
	</xs:complexType>
	<xs:complexType name="ref-text-tp" mixed="true">
		<xs:group minOccurs="0" maxOccurs="unbounded" ref="ani:richstring.data"/>
	</xs:complexType>
	<xs:complexType name="ref-fulltext-tp" mixed="true">
		<xs:group minOccurs="0" maxOccurs="unbounded" ref="ani:richstring.data"/>
	</xs:complexType>
	<xs:complexType name="refd-itemcitation-tp">
		<xs:choice>
			<xs:sequence>
				<xs:element minOccurs="0" ref="ce:pii"/>
				<xs:element minOccurs="0" ref="ce:doi"/>
				<xs:element name="citation-title" type="xocs:citation-title-tp" minOccurs="0" form="unqualified"/>
				<xs:element name="author-group" type="xocs:author-group-tp" minOccurs="0" form="unqualified"/>
				<xs:element name="sourcetitle" type="xocs:sourcetitle-tp" minOccurs="0" form="unqualified"/>
				<xs:element name="sourcetitle-abbrev" type="xocs:sourcetitle-abbrev-tp" minOccurs="0" form="unqualified"/>
				<xs:element name="issn" type="xocs:issn-tp" minOccurs="0" maxOccurs="unbounded" form="unqualified"/>
				<xs:element name="isbn" type="xocs:isbn-tp" minOccurs="0" maxOccurs="unbounded" form="unqualified"/>
				<xs:element name="codencode" type="xs:string" minOccurs="0" form="unqualified"/>
				<xs:element name="part" type="xs:string" minOccurs="0" form="unqualified"/>
				<xs:element name="publicationyear" type="xocs:publicationyear-tp" minOccurs="0" form="unqualified"/>
				<xs:element name="volisspag" type="xocs:volisspag-tp" minOccurs="0" form="unqualified"/>
				<xs:element name="article-number" type="xs:string" minOccurs="0" form="unqualified"/>
				<xs:element name="website" type="xocs:website-tp" minOccurs="0" form="unqualified"/>
			</xs:sequence>
			<xs:element name="ref-text" type="xocs:ref-text-tp" form="unqualified"/>
		</xs:choice>
		<xs:attribute name="type"/>
	</xs:complexType>

	<xs:simpleType name="iso-code-tp">
		<xs:restriction base="xs:token">
			<xs:pattern value="[a-z]{3}"/>
		</xs:restriction>
	</xs:simpleType>

	<xs:complexType name="date-time-tp">
		<xs:simpleContent>
			<xs:extension base="xs:dateTime">
				<xs:attribute name="yyyymmdd" type="xs:integer" use="optional"/>
			</xs:extension>
		</xs:simpleContent>
	</xs:complexType>

	<xs:complexType name="date-tp">
		<xs:simpleContent>
			<xs:extension base="xs:date">
				<xs:attribute name="yyyymmdd" type="xs:integer" use="optional"/>
			</xs:extension>
		</xs:simpleContent>
	</xs:complexType>

	<xs:complexType name="indexed-date-tp">
		<xs:simpleContent>
			<xs:extension base="xs:dateTime">
				<xs:attribute name="epoch" type="xs:integer" use="required"/>
			</xs:extension>
		</xs:simpleContent>
	</xs:complexType>

	<xs:simpleType name="yes-no-string-tp">
		<xs:restriction base="xs:string">
			<xs:enumeration value="yes"/>
			<xs:enumeration value="no"/>
		</xs:restriction>
	</xs:simpleType>

	<xs:complexType name="subj-areas-tp">
		<xs:annotation>
			<xs:documentation>Contains the subject area code.  There can be more than 1 subject area element.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="subjarea" type="xs:string" minOccurs="1" maxOccurs="unbounded"/>
		</xs:sequence>
	</xs:complexType>

	<xs:complexType name="countries-tp">
		<xs:annotation>
			<xs:documentation>Contains the country name.  There can be more than 1 country element.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="country" type="xs:string" minOccurs="1" maxOccurs="unbounded"/>
		</xs:sequence>
	</xs:complexType>

	<xs:complexType name="languages-tp">
		<xs:annotation>
			<xs:documentation>Contains the language name.  There can be more than 1 language element.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="language" type="xs:string" minOccurs="1" maxOccurs="unbounded"/>
		</xs:sequence>
	</xs:complexType>

	<xs:complexType name="prefnameauids-tp">
		<xs:annotation>
			<xs:documentation>Contains the prefnameauid name.  There can be more than 1 prefnameauid element.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="prefnameauid" type="xs:string" minOccurs="1" maxOccurs="unbounded"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="funding-addon-type">
		<xs:annotation>
			<xs:documentation>Contains the funding addon information obtained from VTW.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="funding-source-document" minOccurs="0" maxOccurs="unbounded">
				<xs:complexType>
					<xs:simpleContent>
						<xs:extension base="xs:string">
							<xs:attribute name="source-document-type" type="xs:string" use="required"/>
						</xs:extension>
					</xs:simpleContent>
				</xs:complexType>
			</xs:element>
			<xs:element name="funding-addon-generated-timestamp" type="xs:string"/>
			<xs:element name="funding-addon-type" type="xs:string" minOccurs="0"/>
			<xs:element name="funding" type="xocs:funding-type" minOccurs="0" maxOccurs="unbounded"/>
			<xs:element name="funding-text" type="xs:string" minOccurs="0" maxOccurs="unbounded"/>
		</xs:sequence>
		<xs:attribute name="has-funding-info" type="xs:boolean" use="required"/>
		<xs:attribute name="pui-match" type="xocs:pui-match-type" use="required"/>
	</xs:complexType>
	<xs:complexType name="funding-type">
		<xs:sequence>
			<xs:element name="funding-agency-matched-string" type="xs:string" minOccurs="0"/>
			<xs:element name="funding-id" type="xs:string" minOccurs="0" maxOccurs="unbounded"/>
			<xs:element name="funding-agency-acronym" type="xs:string" minOccurs="0"/>
			<xs:element name="funding-agency" type="xs:string" minOccurs="0"/>
			<xs:element name="funding-agency-id" type="xs:string" minOccurs="0"/>
			<xs:element name="funding-agency-country" type="xs:string" minOccurs="0"/>
		</xs:sequence>
	</xs:complexType>
	<xs:simpleType name="pui-match-type">
		<xs:annotation>
			<xs:documentation>indicates if the eid matching with pui has the pui as a primary or secondary pui.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:enumeration value="primary"/>
			<xs:enumeration value="secondary"/>
		</xs:restriction>
	</xs:simpleType>

	<xs:complexType name="open-access-type">
		<xs:annotation>
			<xs:documentation>Contains the open-access information obtained from CrossRef.
			                  Changed the "is-open-access type to string so '2' is allowed.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="oa-access-effective-date" minOccurs="0" type="xs:string"/>
			<xs:element name="oa-article-status" minOccurs="0">
				<xs:complexType>
					<xs:simpleContent>
						<xs:extension base="xs:string">
							<xs:attribute name="is-open-access" type="xs:string" use="required"/>
						</xs:extension>
					</xs:simpleContent>
				</xs:complexType>
			</xs:element>
			<xs:element name="oa-user-license" minOccurs="0" type="xs:string"/>
			<xs:element name="upw-open-access" type="xocs:upw-open-access-type" minOccurs="0"/>
		</xs:sequence>
	</xs:complexType>


	<xs:complexType name="upw-open-access-type">
		<xs:annotation>
			<xs:documentation>Contains the open-access information obtained from UPW (oaDOI)</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="upw-oa-article-status" minOccurs="0" type="xs:string"/>
			<xs:element name="upw-timestamp" minOccurs="0" type="xs:string"/>
			<xs:element name="upw-oa-journal-is-oa" minOccurs="0" type="xs:string"/>
			<xs:element name="upw-oa-journal-is-in-doaj" minOccurs="0" type="xs:string"/>
			<xs:element name="upw-best_oa_location" minOccurs="0" type="xocs:upw-oa_location-type"/>
			<xs:element name="upw-oa_locations" minOccurs="0" type="xocs:upw-oa_locations-type"/>
		</xs:sequence>
	</xs:complexType>

	<xs:complexType name="upw-oa_locations-type">

		<xs:sequence>
			<xs:element name="upw-oa_location" minOccurs="0" maxOccurs="unbounded" type="xocs:upw-oa_location-type"/>
		</xs:sequence>
	</xs:complexType>

	<xs:complexType name="upw-oa_location-type">

		<xs:sequence>
			<xs:element name="upw-url" minOccurs="0" type="xs:string"/>
			<xs:element name="upw-pmh_id" minOccurs="0" type="xs:string"/>
			<xs:element name="upw-is_best" minOccurs="0" type="xs:string"/>
			<xs:element name="upw-license" minOccurs="0" type="xs:string"/>
			<xs:element name="upw-updated" minOccurs="0" type="xs:string"/>
			<xs:element name="upw-version" minOccurs="0" type="xs:string"/>
			<xs:element name="upw-evidence" minOccurs="0" type="xs:string"/>
			<xs:element name="upw-host_type" minOccurs="0" type="xs:string"/>
			<xs:element name="upw-url_for_pdf" minOccurs="0" type="xs:string"/>
			<xs:element name="upw-url_for_landing_page" minOccurs="0" type="xs:string"/>
		</xs:sequence>
	</xs:complexType>
</xs:schema>